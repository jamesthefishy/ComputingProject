      'function validnumber( '
      'field,lower, upper, '
      'allowempty )'
      '{'
      #9'var value = field;'
      #9'if ( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9'value '
      '= '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'var valid = false;'
      #9'if( ( value == '
      'undefined || value == "" ) '
      '&& allowempty == '#39'true'#39' )'
      #9'{'
      #9#9'valid = '
      'true;'
      #9'}'
      #9'else if( /^\d+'
      '$/.test( value ) && '
      '!/^0\d+$/.test( value ) )'
      #9'{'
      #9#9'value '
      '= parseInt( value );'
      #9#9'lower '
      '= parseInt( lower );'
      #9#9'upper '
      '= parseInt( upper );'
      #9#9'if ( '
      'value >= lower && value '
      '<= upper )'
      #9#9'{'
      #9#9#9
      'valid = true;'
      #9#9'}'
      #9'}'
      #9'if ( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validregex(field, '
      'blank)'
      '{'
      #9'var regex = '
      'field;'
      #9'if( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9'regex '
      '= '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'var valid = true;'
      ''
      #9'regex = '
      'regex.split( /\n|\r/ );'
      #9'var regexes = '
      'new Array();'
      #9'for( var i=0; i < '
      'regex.length; i++ )'
      #9'{'
      #9#9'if( ( '
      'regex[i].length == 0 ) || ( '
      'regex[i] == '#39'\n'#39' ) ) { '
      'continue };'
      #9#9
      'regexes.push( regex[i] );'
      #9'}'
      #9'var '
      'regexeslength = '
      'regexes.length;'
      ''
      #9'for( var counter '
      '= 0; counter < '
      'regexeslength; counter++ '
      ')'
      #9'{'
      #9#9'if( '
      'regexes[counter] == "" && '
      'blank == '#39'false'#39')'
      #9#9'{'
      #9#9#9
      'valid = false;'
      #9#9#9
      'break;'
      #9#9'}'
      #9#9'else'
      #9#9'{'
      #9#9#9
      'regexes[counter] = '
      'regexes[counter].replace'
      '(/\r/g,"");'
      #9#9#9
      'try'
      #9#9#9
      '{'
      #9#9#9
      #9'var test = new '
      'RegExp( regexes[counter] '
      ');'
      #9#9#9
      '}'
      #9#9#9
      'catch(e)'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      ''
      #9'if( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validpassword'
      '(field, blank, max_length)'
      '{'
      #9'var password = '
      'field;'
      #9'var valid = true;'
      #9'if( max_length '
      '== undefined || isNaN( '
      'max_length ) )'
      #9'{'
      #9#9
      'max_length = 32;'
      #9'}'
      ''
      #9'if ( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'password = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'if( ( password '
      '== undefined || password '
      '== "" ) && blank == '#39'false'#39' '
      ')'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'else if( /'
      '[\s]/.test(password) || '
      'password.length > '
      'max_length )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      ''
      #9'if ( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validurl'
      '(field,require_protocol,blan'
      'k,asterisk)'
      '{'
      #9'var valid = true;'
      #9'var inputval = '
      'field;'
      ''
      #9'if( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      #9
      #9'if( blank == '
      #39'false'#39' && ( inputval == '
      'undefined || inputval == "" '
      ') )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'else'
      #9'{'
      #9#9
      'inputval = inputval.split(/'
      '\n|\r/);'
      ''
      #9#9'var urls '
      '= new Array();'
      #9#9'for( '
      'var i=0; i < '
      'inputval.length; i++ )'
      #9#9'{'
      #9#9#9
      'if( ( inputval[i].length == 0 '
      ') || ( inputval[i] == '#39'\n'#39' ) ) { '
      'continue };'
      #9#9#9
      'urls.push( inputval[i] );'
      #9#9'}'
      #9#9'var '
      'urlslength = urls.length;'
      ''
      #9#9'for( '
      'var i=0; i < urlslength; i++ '
      ')'
      #9#9'{'
      #9#9#9
      'urls[i] = urls[i].replace( /'#39'/, '
      #39#39' );'
      #9#9#9
      #9#9#9
      '// get rid of any *. or .* '
      'asterisk notation in our '
      'wildcard syntax'
      #9#9#9
      '// as the rest of the '
      'validation does not care for '
      'it'
      #9#9#9
      'if( asterisk == '#39'true'#39' )'
      #9#9#9
      '{'
      #9#9#9
      #9'urls[i] = urls'
      '[i].replace( /\.\*/, '#39#39' );'
      #9#9#9
      #9'urls[i] = urls'
      '[i].replace( /\*\./, '#39#39' );'
      #9#9#9
      '}'
      ''
      #9#9#9
      '// Repeated '#39'.'#39' is valid in '
      'query or fragment even if '
      'not in domain or path'
      #9#9#9
      '// So replace these by a '
      'single '#39'.'#39' so don'#39't fall foul of '
      'later tests'
      #9#9#9
      '// Note can'#39't use /g option '
      'to regexp as overlapping '
      'matches'
      #9#9#9
      'var oldurl;'
      #9#9#9
      'do{'
      #9#9#9
      #9'oldurl = urls[i];'
      #9#9#9
      #9'urls[i] = '
      'oldurl.replace( /((\#|\?).*'
      '\.)\.+/, '#39'$1'#39' );'
      #9#9#9
      '}while( urls[i] != oldurl );'
      ''
      #9#9#9
      'if( /(-\.)|(\.-)/.test(urls[i]) '
      '|| '
      #9#9#9
      #9'/\.\./.test(urls[i]) '
      '|| '
      #9#9#9
      #9'/[\{\}|\\^'
      '\[\]'#39'`]/.test(urls[i]) ||'
      #9#9#9
      #9'/\/\/$/.test(urls'
      '[i]) ||'
      #9#9#9
      #9'/^\./.test(urls[i]) '
      ')'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'#9
      #9#9#9
      'else if( require_protocol '
      '== '#39'true'#39' && !'
      'urlregex_forcehttp.test'
      '(urls[i]) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9#9
      'else if( require_protocol '
      '== '#39'false'#39' && !'
      'urlregex.test(urls[i]) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9#9
      'else if( /^https?:\/[^'
      '\/]/i.test(urls[i]) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      ''
      #9'if( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validurlpart( field, '
      'blank )'
      '{'
      #9'var valid = true;'
      #9'var inputval = '
      'field;'
      ''
      #9'if( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'inputval = '
