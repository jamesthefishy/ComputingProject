      #39'false'#39';'
      #9#9'}'
      '        }'
      '}'
      ''
      'function menuover(obj)'
      '{'
      #9
      'obj.style.backgroundColor '
      '= "#30305f";'
      #9'obj.style.color = '
      '"#f0f0ff";'
      '}'
      ''
      'function menuout(obj)'
      '{'
      #9
      'obj.style.background = '
      '"none";'
      #9'obj.style.color = '
      '"black";'
      '}'
      ''
      ''
      ''
      '// Validation Functions'
      ''
      'var errored_colour = '
      '"#FFdddd";'
      'var warning_colour = '
      '"#FFDAB9";'
      'var normal_colour = '
      '"white";'
      ''
      '// valid_ip so can be '
      'selective about accepting '
      'mask or range info, if '
      'second arg'
      '// is true then will only take '
      'a distinct single IP address'
      '// if third arg is true then '
      'the field is permitted to be '
      'empty'
      ''
      '// Vip Function takes in a '
      'string and checks to see if '
      'it is an IP address in dotted'
      '// quad formation ...  i.e.   '
      '123.123.123.123 or '
      'similar.'
      ''
      'function __vip( address )'
      '{'
      #9'var numbers = '
      'address.split( "." );'
      ''
      #9'if ( '
      'numbers.length != 4 ){'
      #9#9'return '
      'false;'
      #9'}'
      ''
      #9'for ( var number '
      '= 0 ; number < 4 ; '
      'number++ ){'
      #9#9'if(!(/^'
      '(?:0|[1-9][0-9]*)$/.test'
      '(numbers[number])))'
      #9#9'{'
      #9#9#9
      'return false;'
      #9#9'}'
      ''
      #9#9'if'
      '(numbers[number] > 255)'
      #9#9'{'
      #9#9#9
      'return false;'
      #9#9'}'
      #9'}'
      ''
      #9'return true;'
      '}'
      ''
      'function __vnetmask( '
      'netmask )'
      '{'
      #9'if(/^(?:0|[1-9][0'
      '-9]*)$/.test(netmask) && '
      'netmask <= 32)'
      #9'{'
      #9#9'/* Valid '
      'prefix notation */'
      #9#9'return '
      'true;'
      #9'}'
      ''
      #9'/* See if it '
      'matches any of the valid '
      '(well formed) netmasks...'
      #9' * Yes, this is '
      'nicer than the code to '
      'actually parse them.'
      #9'*/'
      ''
      #9'var '
      'VALID_MASKS = ['
      #9#9
      '"0.0.0.0",'
      ''
      #9#9
      '"128.0.0.0",'
      #9#9
      '"192.0.0.0",'
      #9#9
      '"224.0.0.0",'
      #9#9
      '"240.0.0.0",'
      #9#9
      '"248.0.0.0",'
      #9#9
      '"252.0.0.0",'
      #9#9
      '"254.0.0.0",'
      #9#9
      '"255.0.0.0",'
      ''
      #9#9
      '"255.128.0.0",'
      #9#9
      '"255.192.0.0",'
      #9#9
      '"255.224.0.0",'
      #9#9
      '"255.240.0.0",'
      #9#9
      '"255.248.0.0",'
      #9#9
      '"255.252.0.0",'
      #9#9
      '"255.254.0.0",'
      #9#9
      '"255.255.0.0",'
      ''
      #9#9
      '"255.255.128.0",'
      #9#9
      '"255.255.192.0",'
      #9#9
      '"255.255.224.0",'
      #9#9
      '"255.255.240.0",'
      #9#9
      '"255.255.248.0",'
      #9#9
      '"255.255.252.0",'
      #9#9
      '"255.255.254.0",'
      #9#9
      '"255.255.255.0",'
      ''
      #9#9
      '"255.255.255.128",'
      #9#9
      '"255.255.255.192",'
      #9#9
      '"255.255.255.224",'
      #9#9
      '"255.255.255.240",'
      #9#9
      '"255.255.255.248",'
      #9#9
      '"255.255.255.252",'
      #9#9
      '"255.255.255.254",'
      #9#9
      '"255.255.255.255"'
      #9'];'
      ''
      #9'for(var i = 0; i < '
      'VALID_MASKS.length; ++i)'
      #9'{'
      #9#9'if'
      '(netmask === '
      'VALID_MASKS[i])'
      #9#9'{'
      #9#9#9
      'return true;'
      #9#9'}'
      #9'}'
      ''
      #9'return false;'
      '}'
      ''
      'function __vnum( number, '
      'start, end )'
      '{'
      #9'var valid = true;'
      #9'for ( var '
      'character = 0 ; character '
      '< number.length ; '
      'character++ ){'
      #9#9'if ('
      #9#9#9
      '( number.charAt( character '
      ') < '#39'0'#39' ) ||'
      #9#9#9
      '( number.charAt( character '
      ') > '#39'9'#39' ) ){'
      #9#9#9
      'valid = false;'
      #9#9#9
      'break;'
      #9#9'};'
      #9'}'
      #9'if ( number < '
      'start || number > end ){'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'if ( number == "" '
      '){'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function __valid_ip'
      '(address, allowempty, '
      'netmask, range, single)'
      '{'
      '  var errored = false;'
      '  if ( address == undefined '
      '|| address == "" ){'
      '    errored =  (allowempty '
      '!= true);'
      '  } else {'
      '    // check the content of '
      'the address ...'
      ''
      '    // are we looking for an '
      'IP address range ?  these '
      'are in the form'
      '    // 123.123.123.123:124'
      ''
      '    var parts = '
      'address.split( "-" );'
      ''
      '    if ( parts.length > 1 ){'
      '      if ( range == undefined '
      '|| range == false ){'
      #9'// we aren'#39't '
      'allowed ranges, but appear '
      'to have one'
      #9'errored = true;'
      '      } else {'
      #9'// check both '
      'parts, the first should be a '
      'dotted quad IP'
      #9'// the latter '
      'should be an IP address '
      'too'
      #9'if ( parts.length '
      '> 2 ){'
      #9'  errored = true;'
      #9'} else {'
      #9#9'if ( !'
      '__vip( parts[ 0 ] ) ) {'
      #9#9#9
      '/* First IP is invalid */'
      #9#9#9
      'errored = true;'
      #9#9'}'
      #9#9'else if ( '
      '!__vip( parts[ 1 ] ) ) {'
      #9#9#9
      '/* Second IP is invalid */'
      #9#9#9
      'errored = true;'
      #9#9'}'
      #9#9'else{'
      #9#9#9
      '/* Both IPs are valid, '
      'ensure the last IP isn'#39't '
      'lower'
      #9#9#9
      ' * than the first.'
      #9#9#9
      '*/'
      ''
      #9#9#9
      'var first = parts[0].split'
      '(".");'
      #9#9#9
      'var last  = parts[1].split'
      '(".");'
      ''
      #9#9#9
      'for(var i = 0; i < 4; ++i) {'
      #9#9#9
      #9'if(first[i] > last[i]) '
      '{'
      #9#9#9
      #9#9'errored '
      '= true;'
      #9#9#9
      #9#9'break;'
      #9#9#9
      #9'}'
      #9#9#9
      #9'else if(first[i] < '
      'last[i]) {'
      #9#9#9
      #9#9'break;'
      #9#9#9
      #9'}'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      '      }'
      '    } else {'
      '      // not a range then, is '
      'it a subnet ?'
      '      parts = address.split( '
      #39'/'#39' );'
      '      if ( parts.length > 1 ){'
      #9'if ( parts.length '
      '> 2 ){'
      #9'  errored = true;'
      #9'} else {'
      #9'  if ( netmask == '
      'undefined || netmask == '
      'false ){'
      #9'    errored = '
      'true;'
      #9'  } else {'
      #9'    // check that '
      'both parts are valid.  the '
      'first should'
      #9'    // be a dotted '
      'quad IP, the latter either a '
      'dotted'
      #9'    // quad IP or a '
      'number between 0 and 32'
      #9'    if ( __vip( '
      'parts[ 0 ] ) != true ){'
      #9'      errored = '
      'true;'
      #9'    } else {'
      #9'      if ( !'
      '__vnetmask( parts[ 1 ] ) ){'
      #9#9'errored '
      '= true;'
      #9'      }'
      #9'    }'
      #9'  }'
      #9'}'
      ''
      '      } else {'
      #9'// it'#39's not a '
      'subnet either, so it'#39's either '
      'a valid IP, or invalid'
      #9'// was it '
      'supposed to be a subnet ?'
      #9'if ( netmask == '
      'true && single == false ){'
      #9'  errored = true;'
      #9'} else {'
      #9'  if ( __vip( '
      'address ) != true ){'
      #9'    errored = '
      'true;'
      #9'  }'
      #9'}'
      '      }'
      '    }'
      '  }'
      '  return !errored;'
      '}'
      '// outer wrapper - sets '
      'error colours, only do '
      'validation at all if passed '
      'the right object'
      'function valid_ip( '
      'initial_address, '
      'allowempty, netmask, '
      'range, single )'
      '{'
      '  if ( initial_address.value '
      '== undefined ){'
      '    initial_address = '
      'document.getElementById'
      '(initial_address);'
      '  }'
      '  if(initial_address.value != '
      'undefined && '
      'initial_address.style != '
      'undefined) {'
      '    '
      'initial_address.style.backgr'
      'ound = (__valid_ip'
      '(initial_address.value, '
      'allowempty, netmask, '
      'range, single)  ? '
      'normal_colour : '
      'errored_colour);'
      '  }'
      ''
      '}'
      ''
      'function __valid_comment( '
      'comment, allowempty)'
      '{'
      '  var errored = false;'
      '  if(comment == undefined '
      '|| comment == "") {'
      '    errored = (allowempty '
      '!= true);'
      '  }'
      '  else {'
      '    errored = !/^[\w\d\.\-,'
      '\(\)@$!\%\^\&\*=\+_ ]*'
      '$/.test( comment );'
      '  }'
      '  return !errored;'
      '}'
      ''
      'function valid_comment( '
      'item, allowempty )'
      '{'
      '  if ( item.value == '
      'undefined ){'
      '    item = '
      'document.getElementById'
      '(item);'
      '  }'
      '  if(item.value != undefined '
      '&& item.style != '
      'undefined) {'
      '    item.style.background = '
      '(__valid_comment'
      '(item.value, allowempty)  ? '
      'normal_colour : '
      'errored_colour);'
      '  }'
      '}'
      ''
      'function __valid_itemname( '
      'itemname, allowempty )'
      '{'
      '  var errored = false;'
      ''
      '  if(itemname == undefined '
      '|| itemname == "") {'
      '    errored = (allowempty '
      '!= true);'
      '  }'
      '  else {'
      '    errored = !/^[a-zA-Z0-'
      '9_\. ]*$/.test( itemname );'
      '  }'
      '  return !errored;'
      '}'
      'function valid_itemname( '
      'item, allowempty )'
      '{'
      '  if ( item.value == '
      'undefined ){'
      '    item = '
      'document.getElementById'
      '(item);'
      '  }'
      '  if(item.value != undefined '
      '&& item.style != '
      'undefined) {'
      '     item.style.background '
      '= (__valid_itemname'
      '(item.value, allowempty)  ? '
      'normal_colour : '
      'errored_colour);'
      '  }'
      '}'
      ''
      'function __valid_match( '
      'match, value, allowempty )'
      '{'
      '  var errored = false;'
      ''
      '  if(value == undefined || '
      'value == "") {'
      '    errored = (allowempty '
      '!= true);'
      '  }'
      '  else {'
      '    var re = new RegExp( '
      'match );'
      '    var ma = re.exec( value '
      ');'
      '    if ( ma != null ){'
      '       errored = false;'
      '    } else {'
      '       errored = true;'
      '    }'
      '  }'
      '  return !errored;'
      '}'
      ''
      'function valid_match( '
      'match, item, allowempty )'
      '{'
      '  if ( item.value == '
      'undefined ){'
      '    item = '
      'document.getElementById'
      '(item);'
      '  }'
      '  if(item.value != undefined '
      '&& item.style != '
      'undefined) {'
      '     item.style.background '
      '= (__valid_match( match, '
      'item.value, allowempty)  ? '
      'normal_colour : '
      'errored_colour);'
      '  }'
      '}'
      ''
      'function '
      '__valid_username( '
      'username, allowempty )'
      '{'
      '  var errored = false;'
      ''
      '  if(username == '
      'undefined || username == '
      '"") {'
      '    errored = (allowempty '
      '!= true);'
      '  }'
      '  else {'
      '    errored = !/^[a-zA-Z0-'
      '9_\.]*$/.test( username );'
      '  }'
      '  return !errored;'
      '}'
      ''
      'function valid_username( '
      'item, allowempty )'
      '{'
      '  if ( item.value == '
      'undefined ){'
      '    item = '
      'document.getElementById'
      '(item);'
      '  }'
      '  if(item.value != undefined '
      '&& item.style != '
      'undefined) {'
      '     item.style.background '
      '= (__valid_username'
      '(item.value, allowempty)  ? '
      'normal_colour : '
      'errored_colour);'
      '  }'
      '}'
      ''
      'var password_cache = '
      'new Array();'
      ''
      'function valid_password( '
      'pass1id, pass2id, '
      'allowempty ) {'
      ''
      '    var password1 = '
      'document.getElementById'
      '(pass1id);'
      '    var password2 = '
      'document.getElementById'
      '(pass2id);'
      '    var errored = false;'
      '    var p1 = '
      'password1.value;'
      '    var p2 = '
      'password2.value;'
      ''
      '    if( p1 == undefined || '
      'p2 == undefined || (p1 == '
      '"" && p2 == "")) {'
      '        errored =  '
      '(allowempty != true);'
      '    } else {'
      '        errored = !/^[^\s'
      '\"\'#39']*$/.test( p1 );'
      ''
      #9'if ( p1 != p2 ){'
      #9'    '
      'password1.style.backgrou'
      'nd = warning_colour;'
      #9'    '
      'password_cache[ pass1id ] '
      ' = warning_colour;'
      ''
      #9'    if ( '
      'password_cache[ pass2id ] '
      '!= errored_colour ) {'
      #9'        '
      'password2.style.backgrou'
      'nd = warning_colour;'
      #9#9
      'password_cache[ pass2id ] '
      ' = warning_colour;'
      #9'    }'
      #9'}'
      '        else {'
      #9'    '
      'password1.style.backgrou'
      'nd = normal_colour;'
      #9'    '
      'password_cache[ pass1id ] '
      ' = normal_colour;'
      ''
      #9'    if ( '
      'password_cache[ pass2id ] '
      '!= errored_colour ) {'
      #9'        '
      'password2.style.backgrou'
      'nd = normal_colour;'
      #9#9
      'password_cache[ pass2id ] '
      ' = normal_colour;'
      #9'    }'
      #9'}'
      ''
      '    }'
      '    '
      'password1.style.backgrou'
      'nd = password_cache[ '
      'pass1id ] = (errored ? '
      'errored_colour : '
      'normal_colour);'
      '    return !errored;'
      '}'
      ''
      '// valid_port is used for the '
      'user defined port that goes '
      'with a drop down list of '
      'standard ports'
      '// if the dropdown is not at '
      '0 value we dont need to '
      'check this'
      'function __valid_port(port, '
      'portchoices, allowempty)'
      '{'
      '  var errored = false;'
      '  if( portchoices != '
      'undefined && '
      'portchoices.value != "0" ) {'
      '    errored = false;'
      '  } else {'
      '    if ( port == '#39#39' && '
      'allowempty == true ){'
      ''
      '    } else {'
      '       errored = !/^[\d:]+'
      '$/.test( port);'
      ''
      '       if(!errored) {'
      '         errored = (port < 1 || '
      'port > 0xFFFF);'
      '       }'
      '    }'
      '  }'
      '  return !errored;'
      '}'
      ''
      'function valid_port '
      '(portfield, portchoices, '
      'allowempty)'
      '{'
      '    if ( portfield.value == '
      'undefined ){'
      '    '#9'portfield = '
      'document.getElementById'
      '(portfield);'
      '    }'
      ''
      '    if ( portchoices.value == '
      'undefined){'
      '    '#9'portchoices = '
      'document.getElementById'
      '(portchoices);'
      '    }'
      '    if(portfield.value != '
      'undefined && '
      'portfield.style != '
      'undefined) {'
      '      '
      'portfield.style.background '
      '= (__valid_port'
      '(portfield.value, '
      'portchoices, allowempty) ?  '
      ' normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      ''
      '// valid_host_or_ip - what '
      'is entered in this field must '
      'either be an ip address'
      '// or a hostname. Where a '
      'hostname has to conatin '
      'some letters.'
      '// if the optional '
      'allowempty parameter is '
      'present and true'
      '// then the filed is allowed '
      'to be empty, otherwise '
      'that is an error state'
      '// if something is optional '
      'then it can be allowed '
      'empty'
      ''
      'function '
      '__valid_host_or_ip(name, '
      'allowempty, tryip, '
      'allowport)'
      '{'
      '  var errored = false;'
      '  if(name == undefined || '
      'name == "") {'
      '    errored = (allowempty '
      '!= true);'
      '  }'
      ''
      '  else {'
      #9'if (allowport == '
      'undefined) {'
      #9#9
      'allowport = false;'
      #9'}'
      #9'if (allowport) {'
      #9#9'var '
      'parts = name.split(":");'
      #9#9'name '
      '= parts[0];'
      #9#9'if '
      '(parts.length > 1) {'
      #9#9#9
      'if (parts.length > 2 || !/^'
      '\d+$/.test(parts[1])) {'
      #9#9#9
      #9'return false;'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      '    // have something to '
      'check against'
      '    // a hostname is must '
      'contain at least one letter '
      'in each group of numbers'
      '    // otherwise we test it as '
      'a valid_ip instead'
      '    var parts = name.split'
      '(".");'
      '    var len=parts.length;'
      '    for( var part=0; '
      'part<len; part++) {'
      '      // if we only consist of '
      'a-z0-9 and - and each part '
      'has at least one non '
      'number'
      '      if(!(/^[\w\d-]+$/.test'
      '(parts[part]) && /[^'
      '\d]+/.test(parts[part]))) {'
      #9'errored = true;'
      '      }'
      '    }'
      ''
      '    if(errored && tryip == '
      'true) {'
      '      // let valid_ip have a '
      'crack at deciding - but '
      'disallowing port ranges etc.'
      '      errored = !__valid_ip'
      '(name, false, false, false);'
      '    }'
      '  }'
      '  return !errored;'
      '}'
      ''
      '// __valid_host_list - what '
      'is entered in this field must '
      'be a comma-seperated'
      '// list of hostnames. Where '
      'a hostname has to conatin '
      'some letters.'
      '// if the optional '
      'allowempty parameter is '
      'present and true'
      '// then the filed is allowed '
      'to be empty, otherwise '
      'that is an error state'
      '// if something is optional '
      'then it can be allowed '
      'empty'
      'function __valid_host_list'
      '(name, allowempty)'
      '{'
      #9'var errored = '
      'false;'
      ''
      ''
      #9'// DEBUG'
      #9'//var dbg1 = '
      'document.getElementById'
      '('#39'dbg1'#39');'
      #9'//var dbg2 = '
      'document.getElementById'
      '('#39'dbg2'#39');'
      #9'//var dbg3 = '
      'document.getElementById'
      '('#39'dbg3'#39');'
      ''
      #9'if(name == '
      'undefined || name == "")'
      #9'{'
      #9#9'errored '
      '= (allowempty != true);'
      #9'}'
      #9'else'
      #9'{'
      #9#9'var '
      'hosts = name.split(/\s*,'
      '\s*/);'
      #9#9
      '//dbg1.innerHTML = hosts;'
      ''
      #9#9'for(var '
      'host = 0 ; host < '
      'hosts.length ; host++)'
      #9#9'{'
      #9#9#9
      '//dbg2.innerHTML = hosts'
      '[host];'
      #9#9#9
      'var parts = hosts'
      '[host].split(".");'
      ''
      #9#9#9
      '// have something to check '
      'against'
      #9#9#9
      '// a hostname is must '
      'contain at least one letter '
      'in each group of numbers'
      #9#9#9
      '// otherwise we test it as a '
      'valid_ip instead'
      #9#9#9
      'for(var part = 0 ; part < '
      'parts.length ; part++)'
      #9#9#9
      '{'
      #9#9#9
      #9'// replace trailing '
      'and leading spaces in a '
      'part so that trailing '
      'whitespace'
      #9#9#9
      #9'// in the text field '
      'does not light up red and '
      'confuse the user'
      #9#9#9
      #9'parts[part] = '
      'parts[part].replace(/\s*'
      '$/,'#39#39');'
      #9#9#9
      #9'parts[part] = '
      'parts[part].replace(/^'
      '\s*/,'#39#39');'
      #9#9#9
      #9
      '//dbg3.innerHTML = parts'
      '[part];'
      ''
      '      '#9#9'// if we '
      'only consist of a-z0-9 and - '
      'and each part has at least '
      'one non number'
      #9#9#9
      #9'if(!(/^[\w\d-]+'
      '$/.test(parts[part]) && /'
      '[^\d]+/.test(parts[part])))'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'errored '
      '= true;'
      #9#9#9
      #9'}'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      ''
      #9'return !errored;'
      '}'
      ''
      'function valid_host_or_ip '
      '(namefield, allowempty, '
      'allowport)'
      '{'
      '    if ( namefield.value == '
      'undefined ){'
      '        namefield = '
      'document.getElementById'
      '(namefield);'
      '    }'
      ''
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = (__valid_host_or_ip'
      '(namefield.value, '
      'allowempty, true, '
      'allowport) ? normal_colour '
      ': errored_colour);'
      '    }'
      '}'
      ''
      ''
      'function valid_host '
      '(namefield, allowempty)'
      '{'
      '    if ( namefield.value == '
      'undefined ){'
      '        namefield = '
      'document.getElementById'
      '(namefield);'
      '    }'
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = (__valid_host_or_ip'
      '(namefield.value, '
      'allowempty, false) ? '
      'normal_colour : '
      'errored_colour);'
      '    }'
      ''
      '}'
      ''
      'function valid_host_list '
      '(namefield, allowempty)'
      '{'
      #9'if ( '
      'namefield.value == '
      'undefined )'
      #9'{'
      #9#9
      'namefield = '
      'document.getElementById'
      '(namefield);'
      #9'}'
      ''
      '   if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined )'
      #9'{'
      #9#9
      'namefield.style.backgroun'
      'd = (__valid_host_list'
      '(namefield.value, '
      'allowempty) ? '
      'normal_colour : '
      'errored_colour);'
      #9'}'
      '}'
      ''
      'function __valid_number'
      '(value,lower, upper, '
      'allowempty, onlyinteger )'
      '{'
      '  var errored = false;'
      '  if(value == undefined || '
      'value == "") {'
      '    errored = (allowempty '
      '!= true);'
      '  } else {'
      '    var parsedvalue = '
      'parseFloat(value);'
      '    if (onlyinteger && !'
      'Number.isSafeInteger'
      '(parsedvalue)) {'
      '      errored = true;'
      '    }'
      '    else if ( value >= lower '
      '&& value <= upper && '
      'Number.isFinite'
      '(parsedvalue)){'
      '      errored = false;'
      '    } else {'
      '      errored = true;'
      '    }'
      '  }'
      '  return !errored;'
      '}'
      ''
      'function valid_number( '
      'item, lower, upper, '
      'allowempty, onlyinteger )'
      '{'
      '  if ( item.value == '
      'undefined ){'
      '    item = '
      'document.getElementById'
      '(item);'
      '  }'
      '  if(item.value != undefined '
      '&& item.style != '
      'undefined) {'
      '    item.style.background = '
      '(__valid_number'
      '(item.value, lower, upper, '
      'allowempty, onlyinteger)  ? '
      'normal_colour : '
      'errored_colour);'
      '  }'
      '}'
      ''
      '// Takes a <select> '
      'element (or an id for one),'
      '// changes the background '
      'colour if the currently '
      'selected value matches '
      'invalid_value'
      '// - e.g. '#39'Please select an '
      'option'#39' with backend value '
      #39'invalid_value'#39
      '// On first loop it will also '
      'disable any option with a '
      'value that matches '
      'invalid_value'
      'function valid_selected( '
      'select, invalid_value )'
      '{'
      '  if ( select.value == '
      'undefined ){'
      '    select = '
      'document.getElementById'
      '(select);'
      '  }'
      '  if (!'
      'select.dataset.disabled) {'
      '    for ( var i = 0; i < '
      'select.length; i++ ) {'
      '      select[i].disabled = '
      '(select[i].value == '
      'invalid_value) ? true : '
      'false;'
      '    }'
      '    // We only need to loop '
      '& disable once per load - '
      'use a data attribute to '
      'track'
      '    select.dataset.disabled '
      '= 1;'
      '  }'
      '  if ( select.value != '
      'undefined && select.style '
      '!= undefined) {'
      '    select.style.background '
      '= (select.value == '
      'invalid_value) ? '
      'errored_colour : '#39#39';'
      '  }'
      '}'
      ''
      'function __valid_mac '
      '(name, allowempty)'
      '{'
      '  var errored = false;'
      '  if(name == undefined || '
      'name == "") {'
      '    errored = (allowempty '
      '!= true);'
      '  }'
      ''
      '  else {'
      '    var parts = name.split'
      '(":");'
      '    var len=parts.length;'
      '    for( var part=0; '
      'part<len; part++) {'
      '      if(!(/^[0-9a-fA-F]'
      '{2}$/.test(parts[part]))) {'
      #9'errored = true;'
      '      }'
      '    }'
      ''
      '    if ( name.length > 17 ){'
      '      errored = true;'
      '    }'
      '  }'
      '  return !errored;'
      '}'
      ''
      'function valid_mac (item, '
      'allowempty)'
      '{'
      ''
      '    if ( item.value == '
      'undefined ){'
      '    '#9'item = '
      'document.getElementById'
      '(item);'
      '    }'
      '    if(item.value != '
      'undefined && item.style != '
      'undefined) {'
      '      item.style.background '
      '= (__valid_mac'
      '(item.value,allowempty)  ? '
      'normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      'function __valid_email '
      '(name, allowempty)'
      '{'
      '  var errored = false;'
      '  if(name == undefined || '
      'name == "") {'
      '    errored = (allowempty '
      '!= true);'
      '  }'
      ''
      '  else {'
      '    // have something to '
      'check against'
      '    // regex doesn'#39't cover '
      'all, but should cope with '
      '99% of email addreses'
      '    if(!(/^[a-z0-9!#$%&'#39'*'
      '+/=?^_`{|}~-]+(?:\.[a-'
      'z0-9!#$%&'#39'*+/=?^_`{|}'
      '~-]+)*@(?:[a-z0-9](?:[a-'
      'z0-9-]*[a-z0-9])?\.)+[a-z0'
      '-9](?:[a-z0-9-]*[a-z0-9])?'
      '$/.test(name))) {'
      '      errored = true;'
      '    }'
      '  }'
      ''
      '  return !errored;'
      '}'
      ''
      'function valid_email (item, '
      'allowempty)'
      '{'
      ' if (item.value == '
      'undefined ){'
      '   item = '
      'document.getElementById'
      '(item);'
      ' }'
      ' if(item.value != undefined '
      '&& item.style != '
      'undefined) {'
      '   item.style.background = '
      '(__valid_email'
      '(item.value,allowempty)  ? '
      'normal_colour : '
      'errored_colour);'
      ' }'
      '}'
      ''
      'function __valid_url(name)'
      '{'
      '    if (/^http(s)?:\/\/([a-'
      'zA-Z0-9\-.]{1,100}\.[a-'
      'zA-Z]{1,6}|[0-9]{1,3}\.[0'
      '-9]{1,3}\.[0-9]{1,3}\.[0-'
      '9]{1,3})(\:[0-9]{1,5})?\/'
      '[^'#39'"<>]*$/.test(name)) {'
      '        return true;'
      '    }'
      '    return false;'
      '}'
      'function valid_url '
      '(namefield)'
      '{'
      '    if ( namefield.value == '
      'undefined ){'
      '        namefield = '
      'document.getElementById'
      '(namefield);'
      '    }'
      ''
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = (__valid_url'
      '(namefield.value) ? '
      'normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      '// Multi-line validation '
      'functions'
      ''
      'function '
      '__multiline_valid_host_or_i'
      'p(name, allowpartial)'
      '{'
      '  name = name.replace(/'
      '\r/g,"");'
      '  var parts = name.split'
      '("\n");'
      '  for (var part in parts) {'
      '    if (/^\s*$/.test(parts'
      '[part])) {'
      '      continue;'
      '    }'
      '    if (allowpartial && parts'
      '[part][0] == '#39'.'#39' && parts'
      '[part].length > 1) {'
      '      parts[part] = parts'
      '[part].substr(1);'
      '    }'
      '    if (!__valid_host_or_ip'
      '(parts[part], true, true)) {'
      '      return false;'
      '    }'
      '  }'
      '  return true;'
      '}'
      ''
      'function '
      'multiline_valid_host_or_ip '
      '(namefield, allowpartial)'
      '{'
      '    if ( namefield.value == '
      'undefined ){'
      '        namefield = '
      'document.getElementById'
      '(namefield);'
      '    }'
      ''
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = '
      '(__multiline_valid_host_or_'
      'ip(namefield.value, '
      'allowpartial) ? '
      'normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      'function '
      '__multiline_valid_ip(name, '
      'netmask, range, single)'
      '{'
      '  name = name.replace(/'
      '\r/g,"");'
      '  var parts = name.split'
      '("\n");'
      ''
      '  var len=parts.length;'
      '  for( var part=0; '
      'part<len; part++) {'
      '    if (/^\s*$/.test(parts'
      '[part])) {'
      '      continue;'
      '    }'
      '    if (!__valid_ip(parts'
      '[part], true, netmask, '
      'range, single)) {'
      '      return false;'
      '    }'
      '  }'
      '  return true;'
      '}'
      ''
      'function multiline_valid_ip '
      '(namefield, netmask, '
      'range, single)'
      '{'
      '    if ( namefield.value == '
      'undefined ){'
      '        namefield = '
      'document.getElementById'
      '(namefield);'
      '    }'
      ''
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = (__multiline_valid_ip'
      '(namefield.value, netmask, '
      'range, single) ? '
      'normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      'function '
      '__multiline_valid_url(name)'
      '{'
      '  name = name.replace(/'
      '\r/g,"");'
      '  var parts = name.split'
      '("\n");'
      '  var len=parts.length;'
      '  for( var part=0; '
      'part<len; part++) {'
      '    if (/^\s*$/.test(parts'
      '[part])) {'
      '      continue;'
      '    }'
      '    if (!/^([a-zA-Z0-9\-.]'
      '{1,100})\.([a-zA-Z]'
      '{1,6})\/(.+)$/.test(parts'
      '[part]) && !__valid_ip'
      '(parts'
      '[part],true,false,false,fals'
      'e)) {'
      '      return false;'
      '    }'
      '  }'
      '  return true;'
      '}'
      ''
      'function multiline_valid_url '
      '(namefield)'
      '{'
      '    if ( namefield.value == '
      'undefined ){'
      '        namefield = '
      'document.getElementById'
      '(namefield);'
      '    }'
      ''
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = (__multiline_valid_url'
      '(namefield.value) ? '
      'normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      'function valid_regexp'
      '(name)'
      '{'
      '  if (name.value == '
      'undefined ){'
      '    name = '
      'document.getElementById'
      '(name);'
      '  }'
      '  if (name.value != '
      'undefined && name.style '
      '!= undefined) {'
      '    try {'
      '      var dummy = new '
      'RegExp(name.value);'
      '      name.style.background '
      '= normal_colour;'
      '    } catch (e) {'
      '      name.style.background '
      '= errored_colour;'
      '    }'
      '  }'
      '}'
      ''
      'function '
      '__multiline_valid_regexp'
      '(name)'
      '{'
      '  name = name.replace(/'
      '\r/g,"");'
      '  var parts = name.split'
      '("\n");'
      '  var len=parts.length;'
      '  for( var part=0; '
      'part<len; part++) {'
      '    if (/^\s*$/.test(parts'
      '[part])) {'
      '      continue;'
      '    }'
      '    try {'
      '      var dummy = new '
      'RegExp(parts[part]);'
      '    } catch (e) {'
      '      return false;'
      '    }'
      '  }'
      '  return true;'
      '}'
      'function '
      'multiline_valid_regexp '
      '(namefield)'
      '{'
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = '
      '(__multiline_valid_regexp'
      '(namefield.value) ? '
      'normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      'function '
      '__multiline_valid_mimetype'
      '(name)'
      '{'
      '  name = name.replace(/'
      '\r/g,"");'
      '  var parts = name.split'
      '("\n");'
      '  var len=parts.length;'
      '  for( var part=0; '
      'part<len; part++) {'
      '    if (/^\s*$/.test(parts'
      '[part])) {'
      '      continue;'
      '    }'
      '    if (!/^[a-zA-Z\-0-9\.]+'
      '\/[a-zA-Z\-0-9\.]+$/.test'
      '(parts[part])) {'
      '      return false;'
      '    }'
      '  }'
      '  return true;'
      '}'
      'function '
      'multiline_valid_mimetype '
      '(namefield)'
      '{'
      '    if ( namefield.value == '
      'undefined ){'
      '        namefield = '
      'document.getElementById'
      '(namefield);'
      '    }'
      ''
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = '
      '(__multiline_valid_mimetyp'
      'e(namefield.value) ? '
      'normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      'function '
      '__multiline_valid_extension'
      '(name)'
      '{'
      '  name = name.replace(/'
      '\r/g,"");'
      '  var parts = name.split'
      '("\n");'
      '  var len=parts.length;'
      '  for( var part=0; '
      'part<len; part++) {'
      '    if (/^\s*$/.test(parts'
      '[part])) {'
      '      continue;'
      '    }'
      '    if (!/^\.[^\s]+$/.test'
      '(parts[part])) {'
      '      return false;'
      '    }'
      '  }'
      '  return true;'
      '}'
      ''
      'function '
      'multiline_valid_extension '
      '(namefield)'
      '{'
      '    if ( namefield.value == '
      'undefined ){'
      '        namefield = '
      'document.getElementById'
      '(namefield);'
      '    }'
      ''
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = '
      '(__multiline_valid_extensio'
      'n(namefield.value) ? '
      'normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      'function '
      '__validate_address_domai'
      'n (name, allowempty)'
      '{'
      '   var errored = false;'
      ''
      '   if(name == undefined || '
      'name == "")'
      '   {'
      '      errored = (allowempty '
      '!= true);'
      '   }'
      '   else'
      '   {'
      '      if(!__valid_email(name, '
      'true))'
      '      {'
      '         if(!'
      '__valid_host_or_ip(name, '
      'true, false))'
      '         {'
      '            return false;'
      '         }'
      '         else'
      '         {'
      '            return true;'
      '         }'
      '      }'
      '   }'
      ''
      '   return !errored;'
      '}'
      ''
      'function '
      'validate_address_domain '
      '(namefield, allowempty)'
      '{'
      '    if ( namefield.value == '
      'undefined ){'
      '        namefield = '
      'document.getElementById'
      '(namefield);'
      '    }'
      ''
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = '
      '(__validate_address_domai'
      'n(namefield.value, '
      'allowempty) ? '
      'normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      'function __validate_domain '
      '(name, allowempty)'
      '{'
      '   var errored = false;'
      ''
      '   if(name == undefined || '
      'name == "")'
      '   {'
      '      errored = (allowempty '
      '!= true);'
      '   }'
      ''
      '   if (!/^[a-zA-Z0-9\-]+\.'
      '[a-zA-Z0-9\-]+(\.[a-zA-Z0'
      '-9\-]+)*$/.test(name))'
      '   {'
      '         errored = true;'
      '   }'
      ''
      '   return !errored;'
      '}'
      ''
      'function validate_domain '
      '(namefield)'
      '{'
      '    if ( namefield.value == '
      'undefined ){'
      '        namefield = '
      'document.getElementById'
      '(namefield);'
      '    }'
      ''
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = (__validate_domain'
      '(namefield.value) ? '
      'normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      'function '
      '__multiline_validate_addre'
      'sses (name)'
      '{'
      '  name = name.replace(/'
      '\r/g,"");'
      '   var parts = name.split'
      '("\n");'
      ''
      '   var len=parts.length;'
      '   for( var part=0; '
      'part<len; part++) {'
      '      // ignore blank lines'
      '      if (/^\s*$/.test(parts'
      '[part])) { continue; }'
      ''
      '      if(!__valid_email(parts'
      '[part], true))'
      '      {'
      '         return false;'
      '      }'
      '   }'
      ''
      '   return true;'
      '}'
      ''
      'function '
      '__multiline_validate_addre'
      'sses_domains (name)'
      '{'
      '  name = name.replace(/'
      '\r/g,"");'
      '   var parts = name.split'
      '("\n");'
      ''
      '   var len=parts.length;'
      '   for( var part=0; '
      'part<len; part++) {'
      '      // ignore blank lines'
      '      if (/^\s*$/.test(parts'
      '[part])) { continue; }'
      ''
      '      if(!(__valid_email'
      '(parts[part], true) || '
      '__valid_host_or_ip(parts'
      '[part], true, false)))'
      '      {'
      '         return false;'
      '      }'
      '   }'
      ''
      '   return true;'
      '}'
      ''
      'function '
      'multiline_validate_addresse'
      's (namefield)'
      '{'
      '    if ( namefield.value == '
      'undefined ){'
      '        namefield = '
      'document.getElementById'
      '(namefield);'
      '    }'
      ''
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = '
      '(__multiline_validate_addre'
      'sses(namefield.value) ? '
      'normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      'function '
      'multiline_validate_addresse'
      's_domains (namefield)'
      '{'
      '    if ( namefield.value == '
      'undefined ){'
      '        namefield = '
      'document.getElementById'
      '(namefield);'
      '    }'
      ''
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = '
      '(__multiline_validate_addre'
      'sses_domains'
      '(namefield.value) ? '
      'normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      'function __valid_numeric '
      '(name, empty, spaces) {'
      '    if (empty && '
      'name.length == 0) {'
      '      return true;'
      '    }'
      '    if (spaces) {'
      '      if (/^[0-9\(\) \+]+'
      '$/.test(name)) {'
      '        return true;'
      '      }'
      '    } else {'
      '      if (/^\d+$/.test(name)) '
      '{'
      '        return true;'
      '      }'
      '    }'
      '    return false;'
      '}'
      'function valid_numeric '
      '(namefield, empty, spaces) '
      '{'
      '    if (namefield.value == '
      'undefined) {'
      '      namefield = '
      'document.getElementById'
      '(namefield);'
      '    }'
      '    if(namefield.value != '
      'undefined && '
      'namefield.style != '
      'undefined ) {'
      '      '
      'namefield.style.backgroun'
      'd = (__valid_numeric'
      '(namefield.value, empty, '
      'spaces) ? normal_colour : '
      'errored_colour);'
      '    }'
      '}'
      ''
      ''
      '/* Version 3.0 variations '
      '---------------------------------'
      '----------------- */'
      '/* These versions are '
      'intended to gradually '
      'replace those above and '
      'are      */'
      '/* functional duplicates of '
      'their perl counterparts in '
      'so much as javascript */'
      '/* allows                               '
      '                                     */'
      ''
      ''
      '/* Helpful functions to '
      'define actions */'
      ''
      'function _disable(field)'
      '{'
      #9'if ( '
      'document.getElementById'
      '(field) ){'
      #9#9
      'document.getElementById'
      '(field).disabled = true;'
      #9'}'
      '}'
      ''
      'function _enable(field)'
      '{'
      #9'if ( '
      'document.getElementById'
      '(field) ){'
      #9#9
      'document.getElementById'
      '(field).disabled = false;'
      #9'}'
      '}'
      ''
      'function _error(field)'
      '{'
      #9'if ( '
      'document.getElementById'
      '(field) ){'
      #9#9
      'document.getElementById'
      '(field).style.backgroundCol'
      'or = '#39'#FFdddd'#39';'
      #9'}'
      '}'
      ''
      'function _ok(field)'
      '{'
      #9'if ( '
      'document.getElementById'
      '(field) ){'
      #9#9
      'document.getElementById'
      '(field).style.backgroundCol'
      'or = '#39'white'#39';'
      #9'}'
      '}'
      ''
      '/* Type-specific control '
      'mechanisms */'
      ''
      'function portlist( selectf, '
      'inputf, enabledon, '
      'allowblank )'
      '{'
      #9'var selectval = '
      'document.getElementById'
      '(selectf).value;'
      #9'var inputval  = '
      'document.getElementById'
      '(inputf).value;'
      ''
      #9'if ( selectval == '
      'enabledon ){'
      #9#9
      '_enable(inputf);'
      #9#9
      'validport'
      '(inputf,allowblank);'
      #9'} else {'
      #9#9'_ok'
      '(inputf);'
      #9#9
      '_disable(inputf);'
      #9'}'
      '}'
      ''
      '/* validation functions */'
      ''
      'function validport'
      '(field,blank)'
      '{'
      #9'var port = field;'
      #9'if( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9'port = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      #9'var valid = '
      'validportvalue( port, blank '
      ');'
      ''
      #9'if ( valid ){'
      #9#9'_ok( '
      'field );'
      #9'} else {'
      #9#9'_error( '
      'field );'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validportvalue( '
      'inputval, blank )'
      '{'
      #9'var valid = true;'
      ''
      #9'if( inputval == '
      'undefined || inputval == "" '
      ')'
      #9'{'
      #9#9'if( '
      'blank == '#39'false'#39' )'
      #9#9'{'
      #9#9#9
      'valid = false;'
      #9#9'}'
      #9'}'
      #9'else'
      #9'{'
      #9#9
      'inputval = inputval.split(/'
      '\n|\r/);'
      #9#9'var '
      'ports = new Array();'
      #9#9'for( '
      'var i=0; i < '
      'inputval.length; i++ )'
      #9#9'{'
      #9#9#9
      'if( ( inputval[i].length == 0 '
      ') || ( inputval[i] == '#39'\n'#39' ) ) { '
      'continue };'
      #9#9#9
      'ports.push( inputval[i] );'
      #9#9'}'
      #9#9'var '
      'portslength = ports.length;'
      ''
      #9#9'for( '
      'var counter = 0 ; counter '
      '< portslength ; counter++ '
      ')'
      #9#9'{'
      #9#9#9
      '/* check it for validity */'
      #9#9#9
      'if( ports[counter] == "" && '
      'allowblank == '#39'false'#39')'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9#9
      'if( isNaN( ports[counter] ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'//given value is '
      'not a number'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9#9
      'if( ports[counter] < 1 || '
      'ports[counter] > 0xFFFF )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validip(field,blank)'
      '{'
      #9'var inputval = '
      'field;'
      #9'if ( '
      'document.getElementById'
      '(field) ){'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      #9'var valid = '
      'validipvalue(inputval, '
      'blank);'
      ''
      #9'if ( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function '
      'validprotocolandiporipandp'
      'ort(field, blank)'
      '{'
      #9'var '
      'protocolandiporipandport = '
      'document.getElementById'
      '(field).value;'
      #9'var valid = false;'
      ''
      #9'if( '
      'protocolandiporipandport '
      '&& '
      'protocolandiporipandport '
      '!= "" )'
      #9'{'
      #9#9'if( '
      'protocolandiporipandport '
      '!= "" )'
      #9#9'{'
      #9#9#9
      'var detail_finder = new '
      'RegExp( /^(?:http|'
      'https)\:\/\/(.*)$/ );'
      #9#9#9
      #9#9#9
      'var matches = '
      'detail_finder.exec( '
      'protocolandiporipandport );'
      #9#9#9
      'if( matches )'
      #9#9#9
      '{'
      #9#9#9
      #9'if( validipvalue( '
      'matches[1] ) )'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'valid = '
      'true;'
      #9#9#9
      #9'}'
      #9#9#9
      #9'else if( '
      'validipandportvalue( '
      'matches[1] ) )'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'valid = '
      'true;'
      #9#9#9
      #9'}'
      #9#9#9
      '}'
      #9#9'}'
      #9#9'else if( '
      'protocolandiporipandport '
      '== "" && blank == '#39'true'#39' )'
      #9#9'{'
      #9#9#9
      'valid = true;'
      #9#9'}'
      #9'}'
      ''
      #9'if ( valid ){'
      #9#9'_ok( '
      'field );'
      #9'} else {'
      #9#9'_error( '
      'field );'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validiporipandport'
      '(field, blank)'
      '{'
      #9'var valid = false;'
      ''
      #9'if( validip( field, '
      'blank ) )'
      #9'{'
      #9#9'valid = '
      'true;'
      #9'}'
      #9'else if( '
      'validipandport( field, blank '
      ') )'
      #9'{'
      #9#9'valid = '
      'true;'
      #9'}'
      ''
      #9'if ( valid ){'
      #9#9'_ok( '
      'field );'
      #9'} else {'
      #9#9'_error( '
      'field );'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validipvalue'
      '(inputval, blank)'
      '{'
      #9'var valid = true;'
      #9'if ( inputval == "" '
      '&& blank == '#39'false'#39' )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'else'
      #9'{'
      #9#9
      'inputval = inputval.split(/'
      '\n|\r/);'
      #9#9'var '
      'addresses = new Array();'
      #9#9'for( '
      'var i=0; i < '
      'inputval.length; i++ )'
      #9#9'{'
      #9#9#9
      'if( ( inputval[i].length == 0 '
      ') || ( inputval[i] == '#39'\n'#39' ) ) { '
      'continue };'
      #9#9#9
      'addresses.push( inputval[i] '
      ');'
      #9#9'}'
      #9#9'var '
      'addresseslength = '
      'addresses.length;'
      ''
      #9#9'if( '
      'typeof( addresseslength ) '
      '!= '#39'undefined'#39' && '
      'addresseslength > 0 )'
      #9#9'{'
      #9#9#9
      'var counter = 0;'
      #9#9#9
      '/*do while used to check if '
      'error has occurred before '
      'attempting to'
      #9#9#9
      'loop to next given ip '
      'address and instead print '
      'out error message,'
      #9#9#9
      'stating that given ip list '
      'contains invalid data*/'
      #9#9#9
      'do'
      #9#9#9
      '{'
      #9#9#9
      #9'/* Explictly '
      'allowing 0.0.0.0 as this is '
      'the default value on the '
      'zone'
      #9#9#9
      #9'bridging page '
      'when no source or '
      'destination range has been '
      'provided.'
      #9#9#9
      #9'Blocking this '
      'directly stops other 0 '
      'leading IPs across the '
      'interface. */'
      #9#9#9
      #9'if(! /^[1-9]([0-'
      '9]{1,2})?\.([0-9]{1,3})\.'
      '([0-9]{1,3})\.([0-9]'
      '{1,3})$/.test( addresses'
      '[counter] )'
      #9#9#9
      #9#9#9
      '&& addresses[counter] != '
      #39'0.0.0.0'#39')'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'valid = '
      'false;'
      #9#9#9
      #9'}'
      #9#9#9
      #9'else'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'var '
      'numbers = addresses'
      '[counter].split( "." );'
      #9#9#9
      #9#9'for ( '
      'var number = 0 ; number < '
      '4 ; number++ )'
      #9#9#9
      #9#9'{'
      #9#9#9
      #9#9#9
      'if (( numbers[ number ] < 0 '
      ') || ( numbers[ number ] > '
      '255 ))'
      #9#9#9
      #9#9#9
      '{'
      #9#9#9
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9#9#9
      '}'
      #9#9#9
      #9#9'}'
      #9#9#9
      #9'}'
      #9#9#9
      #9'counter++;'
      #9#9#9
      '}while( (counter < '
      'addresseslength) && (valid '
      '!= false) )'
      #9#9'}'
      #9'}'
      ''
      #9'return valid;'
      '}'
      ''
      'function validiprange( field, '
      'blank )'
      '{'
      #9'var addresses = '
      'field;'
      #9'if ( '
      'document.getElementById'
      '(field) ){'
      #9#9
      'addresses = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      #9'var valid = false;'
      ''
      #9'if( typeof( '
      'addresses ) != '#39'undefined'#39' '
      ')'
      #9'{'
      #9#9'var '
      'parts = addresses.split( "-" '
      ');'
      ''
      #9#9'if( '
      'parts.length == 2 && '
      'parts[0] && validipvalue( '
      'parts[ 0 ] ) &&'
      #9#9#9
      'parts[1] && validipvalue( '
      'parts[ 1 ] ) )'
      #9#9'{'
      #9#9#9
      'valid = true;'
      #9#9'}'
      #9'}'
      ''
      #9'if ( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      ''
      #9'return valid;'
      '}'
      ''
      'function validmask'
      '(field,blank)'
      '{'
      #9'var inputval = "";'
      #9'if ( '
      'document.getElementById'
      '(field) ){'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      #9'var valid = '
      'validmaskvalue(field, '
      'blank);'
      ''
      #9'if ( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validmaskvalue'
      '(inputval, blank)'
      '{'
      #9'var mask = '
      'inputval;'
      #9'var valid = true;'
      ''
      #9'if ( mask == "" '
      '&& blank == '#39'true'#39' )'
      #9'{'
      #9#9'valid = '
      'true;'
      #9'}'
      #9'else'
      #9'{'
      #9#9'// is it a '
      'valid ip ?'
      #9#9'if ( '
      'validipvalue( mask, blank ) '
      ')'
      #9#9'{'
      #9#9#9
      'valid = true;'
      #9#9'}'
      #9#9'else if ( '
      '( /^[1-9][0-9]*$/.test( '
      'mask ) || mask == '#39'0'#39' ) && '
      'mask >= 0 && mask <= 32 '
      ')'
      #9#9'{'
      #9#9#9
      'valid = true;'
      #9#9'}'
      #9#9'else'
      #9#9'{'
      #9#9#9
      'valid = false;'
      #9#9'}'
      #9'}'
      ''
      #9'return valid;'
      '}'
      ''
      'function validipormask'
      '(field, blank)'
      '{'
      #9'var ipormask = '
      'field;'
      #9'if ( '
      'document.getElementById'
      '(field) ){'
      #9#9
      'ipormask = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'var valid = true;'
      #9
      #9'if ( ipormask == '
      '"" && blank == '#39'true'#39' )'
      #9'{'
      #9#9'valid = '
      'true;'
      #9'}'
      #9'else'
      #9'{'
      #9#9'// is it '
      'an ip only ?'
      #9#9'if ( '
      'validipvalue( field, blank ) )'
      #9#9'{'
      #9#9#9
      'valid = true;'
      #9#9'}'
      #9#9'else'
      #9#9'{'
      #9#9#9
      '/* split it into a number and '
      'a mask */'
      #9#9#9
      'var detail_finder = new '
      'RegExp( /^(.*)\/(.*)$/ );'
      #9#9#9
      'var matches = '
      'detail_finder.exec( '
      'ipormask );'
      #9#9#9
      'if ( !matches )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      '}'
      #9#9#9
      'else'
      #9#9#9
      '{'
      #9#9#9
      #9'if ( !validipvalue'
      '(matches[1]) )'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'valid = '
      'false;'
      #9#9#9
      #9'}'
      #9#9#9
      #9'else'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'valid = '
      'validmask(matches[2]);'
      #9#9#9
      #9'}'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      ''
      #9'if ( valid ){'
      #9#9'_ok( '
      'field );'
      #9'} else {'
      #9#9'_error( '
      'field );'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validipandmask'
      '(field, blank)'
      '{'
      #9'var ipandmask = '
      'field;'
      #9'if ( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'ipandmask = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'var valid = true;'
      #9
      #9'if ( ipandmask '
      '== "" && blank == '#39'true'#39' )'
      #9'{'
      #9#9'valid = '
      'true;'
      #9'}'
      #9'else'
      #9'{'
      #9#9'/* split '
      'it into a number and a mask '
      '*/'
      #9#9
      'ipandmask = '
      'ipandmask.split( '#39'/'#39' );'
      ''
      #9#9'/* '
      'check IP and mask have '
      'been provided */'
      #9#9'if( '
      'ipandmask.length != 2 || '
      'ipandmask[0] == '#39#39' || '
      'ipandmask[1] == '#39#39' )'
      #9#9'{'
      #9#9#9
      'valid = false;'
      #9#9'}'
      #9#9'else'
      #9#9'{'
      #9#9#9
      'if( !validipvalue( '
      'ipandmask[0] ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      '}'
      #9#9#9
      'else'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = '
      'validmaskvalue( '
      'ipandmask[1] );'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      ''
      #9'if ( valid ){'
      #9#9'_ok( '
      'field );'
      #9'} else {'
      #9#9'_error( '
      'field );'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validipandport'
      '(field, blank)'
      '{'
      #9'var ipandport = '
      'field;'
      #9'if( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'ipandport = '
