      'function validdomain( '
      'field,require_protocol,blank'
      ',wildcard,asterisk )'
      '{'
      #9'var valid = true;'
      #9'var inputval = '
      'field;'
      ''
      #9'if( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      #9
      #9'if( blank == '
      #39'false'#39' && ( inputval == '
      'undefined || inputval == "" '
      ') )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'else'
      #9'{'
      #9#9
      'inputval = inputval.split(/'
      '\n|\r/);'
      ''
      #9#9'var '
      'domains = new Array();'
      #9#9'for( '
      'var i=0; i < '
      'inputval.length; i++ )'
      #9#9'{'
      #9#9#9
      'if( ( inputval[i].length == 0 '
      ') || ( inputval[i] == '#39'\n'#39' ) ) { '
      'continue };'
      #9#9#9
      'domains.push( inputval[i] );'
      #9#9'}'
      #9#9'var '
      'domainslength = '
      'domains.length;'
      ''
      #9#9'for( '
      'var i = 0 ; i < '
      'domainslength ; i++ )'
      #9#9'{'
      #9#9#9
      '// get rid of any "." prefix if '
      'we are accepting wildcards'
      #9#9#9
      '// as the rest of the '
      'validation does not care for '
      'it'
      #9#9#9
      'if( wildcard == '#39'true'#39' && '
      '/^\./.test(domains[i]) )'
      #9#9#9
      '{'
      #9#9#9
      #9'domains[i] = '
      'domains[i].substr(1);'
      #9#9#9
      '}'
      ''
      #9#9#9
      '// get rid of any *. or .* '
      'asterisk notation in our '
      'wildcard syntax'
      #9#9#9
      '// as the rest of the '
      'validation does not care for '
      'it'
      #9#9#9
      'if( asterisk == '#39'true'#39' )'
      #9#9#9
      '{'
      #9#9#9
      #9'domains[i] = '
      'domains[i].replace( /\.\*/, '#39#39' '
      ');'
      #9#9#9
      #9'domains[i] = '
      'domains[i].replace( /\*\./, '#39#39' '
      ');'
      #9#9#9
      '}'
      #9#9#9
      #9#9#9
      'if( /(-\.)|(\.-)/.test'
      '(domains[i]) || /\.\./.test'
      '(domains[i]) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9#9
      'else if( wildcard != '#39'true'#39' '
      '&& /^\./.test(domains[i]) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9#9
      'else if( require_protocol '
      '== '#39'true'#39' && !'
      'domainregex_forcehttp.tes'
      't(domains[i]) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9#9
      'else if( require_protocol '
      '== '#39'false'#39' && !'
      'domainregex.test(domains'
      '[i]) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9#9
      'else if( /^https?:\/[^'
      '\/]/i.test(domains[i]) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      ''
      #9'if( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validurlordomain( '
      'field,require_protocol,blank'
      ',wildcard )'
      '{'
      #9'var inputval = '
      'field;'
      #9'var valid = false;'
      #9'if( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'if( inputval == "" '
      '&& blank == '#39'true'#39' )'
      #9'{'
      #9#9'valid = '
      'true;'
      #9'}'
      #9'else if( inputval '
      '!= "" )'
      #9'{'
      #9#9
      'inputval = inputval.split(/'
      '\n|\r/);'
      #9#9'var '
      'url_domains = new Array'
      '();'
      #9#9'for( '
      'var i=0; i < '
      'inputval.length; i++ )'
      #9#9'{'
      #9#9#9
      'if( ( inputval[i].length == 0 '
      ') || ( inputval[i] == '#39'\n'#39' ) ) { '
      'continue };'
      #9#9#9
      'url_domains.push( '
      'inputval[i] );'
      #9#9'}'
      #9#9'var '
      'url_domainslength = '
      'url_domains.length;'
      ''
      #9#9'var '
      'valid_count = 0;'
      #9#9'for( '
      'var counter=0; counter < '
      'url_domainslength; '
      'counter++ )'
      #9#9'{'
      #9#9#9
      'if( validurl( url_domains'
      '[counter], '
      'require_protocol, blank ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid_count++;'
      #9#9#9
      '}'
      #9#9#9
      'else if( validdomain( '
      'url_domains[counter], '
      'require_protocol, blank, '
      'wildcard ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid_count++;'
      #9#9#9
      '}'
      #9#9'}'
      #9#9'if( '
      'valid_count == '
      'url_domainslength )'
      #9#9'{'
      #9#9#9
      'valid = true;'
      #9#9'}'
      #9'}'
      ''
      #9'if( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function '
      'validbrowserproxyexceptio'
      'n( field,blank )'
      '{'
      #9'var inputval = '
      'field;'
      #9'var valid = false;'
      #9'if( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'if( inputval == "" '
      '&& blank == '#39'true'#39' )'
      #9'{'
      #9#9'valid = '
      'true;'
      #9'}'
      #9'else if( inputval '
      '!= "" )'
      #9'{'
      #9#9
      'inputval = inputval.split(/'
      '\n|\r/);'
      #9#9'var '
      'url_domains = new Array'
      '();'
      #9#9'for( '
      'var i=0; i < '
      'inputval.length; i++ )'
      #9#9'{'
      #9#9#9
      'if( ( inputval[i].length == 0 '
      ') || ( inputval[i] == '#39'\n'#39' ) ) { '
      'continue };'
      #9#9#9
      'url_domains.push( '
      'inputval[i] );'
      #9#9'}'
      #9#9'var '
      'url_domainslength = '
      'url_domains.length;'
      ''
      #9#9'var '
      'valid_count = 0;'
      #9#9'for( '
      'var counter=0; counter < '
      'url_domainslength; '
      'counter++ )'
      #9#9'{'
      #9#9#9
      'if( validurl( url_domains'
      '[counter], '#39'false'#39', blank, '
      #39'true'#39' ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid_count++;'
      #9#9#9
      '}'
      #9#9#9
      'else if( validdomain( '
      'url_domains[counter], '
      #39'false'#39', blank, '#39'false'#39', '#39'true'#39' ) '
      ')'
      #9#9#9
      '{'
      #9#9#9
      #9'valid_count++;'
      #9#9#9
      '}'
      #9#9#9
      'else if( validip( '
      'url_domains[counter], '
      'blank ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid_count++;'
      #9#9#9
      '}'
      #9#9#9
      'else if( validhostname( '
      'url_domains[counter], '
      'blank ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid_count++;'
      #9#9#9
      '}'
      #9#9#9
      'else if( validipandmask( '
      'url_domains[counter], '
      'blank ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid_count++;'
      #9#9#9
      '}'
      #9#9'}'
      #9#9'if( '
      'valid_count == '
      'url_domainslength )'
      #9#9'{'
      #9#9#9
      'valid = true;'
      #9#9'}'
      #9'}'
      ''
      #9'if( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function transliterate( '
      'input,original_chars,new_c'
      'hars )'
      '{'
      #9'var output = "";'
      #9'if( input.length > '
      '0 && original_chars.length '
      '<= new_chars.length )'
      #9'{'
      #9#9'for( '
      'var i=0; i < input.length; '
      'i++ )'
      #9#9'{'
      #9#9#9
      'var char_index = '
      'original_chars.indexOf( '
      'input.charAt(i) );'
      #9#9#9
      'if( i == 0 )'
      #9#9#9
      '{'
      #9#9#9
      #9'output = '
      'new_chars.charAt( '
      'char_index );'
      #9#9#9
      '}'
      #9#9#9
      'else'
      #9#9#9
      '{'
      #9#9#9
      #9'output = output '
      '+ new_chars.charAt( '
      'char_index );'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      #9'return output;'
      '}'
      ''
      'function validphrase( '
      'field,blank,mode )'
      '{'
      #9'var inputval = '
      'field;'
      #9'if( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'if( inputval == "" '
      '&& blank == '#39'false'#39' )'
      #9'{'
      #9#9'_error( '
      'field );'
      #9#9'return '
      'false;'
      #9'}'
      ''
      #9'inputval = '
      'inputval.split(/\n|\r/);'
      #9'var phrases = '
      'new Array();'
      #9'for( var i=0; i < '
      'inputval.length; i++ )'
      #9'{'
      #9#9'if( ( '
      'inputval[i].length == 0 ) || '
      '( inputval[i] == '#39'\n'#39' ) ) { '
      'continue };'
      #9#9
      'phrases.push( inputval[i] );'
      #9'}'
      #9'var '
      'phraseslength = '
      'phrases.length;'
      ''
      #9'var counter=0;'
      #9'do'
      #9'{'
      #9#9'var '
      'current_phrase = phrases'
      '[counter];'
      #9#9'if( /^'
      '[(<[{|]/.test( '
      'current_phrase ) )'
      #9#9'{'
      #9#9#9
      'var delimiter_open = '
      'current_phrase.substr( 0,1 '
      ');'
      #9#9#9
      'var delimiter_close = '
      'transliterate( '
      'delimiter_open,'#39'(<'
      '[{|'#39','#39')>]}|'#39' );'
      ''
      #9#9#9
      '//check phrase ends in a '
      'close delimiter'
      #9#9#9
      'if( !( '
      'current_phrase.charAt( '
      'current_phrase.length - 1 ) '
      '== delimiter_close ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'_error( field );'
      #9#9#9
      #9'return false;'
      #9#9#9
      '}'
      ''
      #9#9#9
      '//convert delimiters to Hex '
      'values for use in regexes'
      #9#9#9
      '//done to resolve mootools '
      'error when entering '#39'['#39
      #9#9#9
      'var delimiter_open_hex = '
      #39'\\x'#39' + '
      'delimiter_open.charCodeAt'
      '(0).toString(16);'
      #9#9#9
      'var delimiter_close_hex = '
      #39'\\x'#39' + '
      'delimiter_close.charCodeAt'
      '(0).toString(16);'
      ''
      #9#9#9
      'var delimiter_regex = new '
      'RegExp( '
      'delimiter_open_hex'
      '+'#39'|'#39'+delimiter_close_hex,'#39'g'#39' '
      ');'
      #9#9#9
      'var delimiter_open_regex '
      '= new RegExp( '
      'delimiter_open_hex,'#39'g'#39' );'
      #9#9#9
      'var delimiter_close_regex '
      '= new RegExp( '
      'delimiter_close_hex,'#39'g'#39' );'
      ''
      #9#9#9
      '//checks there are '
      'matching delimiters'
      #9#9#9
      'if( delimiter_regex.test'
      '(phrases[counter]) && '
      'delimiter_open_regex.test'
      '(phrases[counter]) && '
      'delimiter_close_regex.test'
      '(phrases[counter]) )'
      #9#9#9
      '{'
      #9#9#9
      #9'var '
      'delimiter_occurances = '
      'current_phrase.match( '
      'delimiter_regex ).length;'
      #9#9#9
      #9'var '
      'delimiter_open_occurances '
      '= current_phrase.match( '
      'delimiter_open_regex '
      ').length;'
      #9#9#9
      #9'var '
      'delimiter_close_occurances '
      '= current_phrase.match( '
      'delimiter_close_regex '
      ').length;'
      ''
      #9#9#9
      #9'if( ( '
      'delimiter_close_occurances '
      '!= '
      'delimiter_open_occurances '
      ') ||'
      #9#9#9
      #9#9'( '
      'delimiter_occurances % 2 '
      '!= 0 ) ||'
      #9#9#9
      #9#9'( '
      'delimiter_occurances != 2 '
      '&& mode == '#39'1'#39' ) )'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'_error( '
      'field );'
      #9#9#9
      #9#9'return '
      'false;'
      #9#9#9
      #9'}'
      #9#9#9
      '}'
      ''
      #9#9#9
      '//check content does not '
      'exist outside of delimiters'
      #9#9#9
      'if( current_phrase.substr( '
      '0,1 ) == '#39'|'#39' )'
      #9#9#9
      '{'
      #9#9#9
      #9'var sections = '
      'new Array;'
      #9#9#9
      #9'var temp = '
      'current_phrase.split( '#39'|'#39' );'
      #9#9#9
      #9'for( var i=0, len '
      '= temp.length; i < len; i++ '
      ')'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'if( '
      'temp[i] == '#39#39' ) { continue };'
      #9#9#9
      #9#9
      'sections.push( temp[i] );'
      #9#9#9
      #9'}'
      #9#9#9
      #9'if( '
      'sections.length != '
      'delimiter_occurances / 2 )'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'return '
      'false;'
      #9#9#9
      #9'}'
      #9#9#9
      '}'
      #9#9#9
      'else'
      #9#9#9
      '{'
      #9#9#9
      #9'var '
      'previous_delimiter = '
      'current_phrase.charAt(0);'
      #9#9#9
      #9'for( var i=1, '
      'phrase_len = '
      'current_phrase.length; i != '
      'phrase_len; i++ )'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'var '
      'current_char = '
      'current_phrase.charAt(i);'
      #9#9#9
      #9#9'var '
      'next_char = '
      'current_phrase.charAt(i'
      '+1);'
      #9#9#9
      #9#9'if( '
      'current_char == '
      'previous_delimiter )'
      #9#9#9
      #9#9'{'
      #9#9#9
      #9#9#9
      '_error( field );'
      #9#9#9
      #9#9#9
      'return false;'
      #9#9#9
      #9#9'}'
      #9#9#9
      #9#9'if( ( '
      'current_char == '
      'delimiter_close ) &&'
      #9#9#9
      #9#9#9
      '( next_char != '
      'delimiter_open ) && i+1 < '
      'phrase_len )'
      #9#9#9
      #9#9'{'
      #9#9#9
      #9#9#9
      '_error( field );'
      #9#9#9
      #9#9#9
      'return false;'
      #9#9#9
      #9#9'}'
      #9#9#9
      #9#9'if( '
      'current_char == '
      'delimiter_open || '
      'current_char == '
      'delimiter_close )'
      #9#9#9
      #9#9'{'
      #9#9#9
      #9#9#9
      'previous_delimiter = '
      'current_char;'
      #9#9#9
      #9#9'}'
      #9#9#9
      #9'}'
      #9#9#9
      '}'
      ''
      #9#9#9
      '//validate weighted phrase'
      #9#9#9
      'if( mode == '#39'3'#39' )'
      #9#9#9
      '{'
      #9#9#9
      #9'//check weight '
      'value has been given'
      #9#9#9
      #9'var phrase_parts '
      '= [];'
      #9#9#9
      #9'var parts = '
      'current_phrase.split( '
      'delimiter_regex );'
      #9#9#9
      #9'for( var i=0; i < '
      'parts.length; i++ )'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'if( '
      'parts[i].length == 0 ) { '
      'continue };'
      #9#9#9
      #9#9
      'phrase_parts.push( parts'
      '[i] );'
      #9#9#9
      #9'}'
      #9#9#9
      #9'if( !/^-?[1-9]{1}'
      '[0-9]{0,2}$/.test( '
      'phrase_parts.pop() ) || '
      'phrase_parts == '#39#39' )'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'_error( '
      'field );'
      #9#9#9
      #9#9'return '
      'false;'
      #9#9#9
      #9'}'
      #9#9#9
      '}'
      ''
      #9#9#9
      'counter++;'
      #9#9'}'
      #9#9'else'
      #9#9'{'
      #9#9#9
      '_error( field );'
      #9#9#9
      'return false;'
      #9#9'}'
      #9'}while( counter < '
      'phraseslength )'
      ''
      #9'_ok( field );'
      #9'return true;'
      '}'
      ''
      'function validfileextension( '
      'field,blank )'
      '{'
      #9'var inputval = '
      'field;'
      #9'var valid = true;'
      ''
      #9'if( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'if( inputval == "" '
      '&& blank == '#39'false'#39' )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'else'
      #9'{'
      #9#9
      'inputval = inputval.split(/'
      '\n|\r/);'
      #9#9'var '
      'extensions = new Array();'
      #9#9'for( '
      'var i=0; i < '
      'inputval.length; i++ )'
      #9#9'{'
      #9#9#9
      'if( ( inputval[i].length == 0 '
      ') || ( inputval[i] == '#39'\n'#39' ) ) { '
      'continue };'
      #9#9#9
      'extensions.push( inputval'
      '[i] );'
      #9#9'}'
      #9#9'var '
      'extensionslength = '
      'extensions.length;'
      ''
      #9#9'for( '
      'var i = 0 ; i < '
      'extensionslength ; i++ )'
      #9#9'{'
      #9#9#9
      'if( !/^\.[a-z0-9]+/.test( '
      'extensions[i] ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      ''
      #9'if( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validhttpheader( '
      'field,blank )'
      '{'
      #9'var inputval = '
      'field;'
      #9'var valid = true;'
      ''
      #9'if( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'if( inputval == "" '
      '&& blank == '#39'false'#39' )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'else'
      #9'{'
      #9#9
      'inputval = inputval.split(/'
      '\n|\r/);'
      #9#9'var '
      'headers = new Array();'
      #9#9'for( '
      'var i=0; i < '
      'inputval.length; i++ )'
      #9#9'{'
      #9#9#9
      'if( ( inputval[i].length == 0 '
      ') || ( inputval[i] == '#39'\n'#39' ) ) { '
      'continue };'
      #9#9#9
      'headers.push( inputval[i] );'
      #9#9'}'
      #9#9'var '
      'headerslength = '
      'headers.length;'
      ''
      #9#9'for( '
      'var i = 0 ; i < '
      'headerslength ; i++ )'
      #9#9'{'
      #9#9#9
      'if( !/^[^:\s]+: \S(?:.*\S)?'
      '$/.test( headers[i] ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      ''
      #9'if( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validmime( '
      'field,blank )'
      '{'
      #9'var inputval = '
      'field;'
      #9'var valid = true;'
      ''
      #9'if( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'if( inputval == '#39#39' '
      '&& blank == '#39'false'#39' )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'else'
      #9'{'
      #9#9
      'inputval = inputval.split(/'
      '\n|\r/);'
      #9#9'var '
      'mimes = new Array();'
      #9#9'for( '
      'var i=0; i < '
      'inputval.length; i++ )'
      #9#9'{'
      #9#9#9
      'if( ( inputval[i].length == 0 '
      ') || ( inputval[i] == '#39'\n'#39' ) ) { '
      'continue };'
      #9#9#9
      'mimes.push( inputval[i] );'
      #9#9'}'
      #9#9'var '
      'mimeslength = '
      'mimes.length;'
      ''
      #9#9'for( '
      'var i = 0 ; i < mimeslength '
      '; i++ )'
      #9#9'{'
      #9#9#9
      'if( !/^[a-z-]+\/[a-'
      'z-]+/.test( mimes[i] ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      ''
      #9'if( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validemail( '
      'field,blank )'
      '{'
      #9'var inputval = '
      'field;'
      #9'var valid = true;'
      ''
      #9'if( '
      'document.getElementById( '
      'field ) )'
      #9'{'
      #9#9
      'inputval = '
      'document.getElementById( '
      'field ).value;'
      #9'}'
      ''
      #9'if( inputval == "" '
      '&& blank == '#39'false'#39' )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'else if( inputval '
      '!= '#39#39' )'
      #9'{'
      #9#9'if( /\.'
      '\./.test( inputval ) || '
      'inputval.split('#39'@'#39').length != '
      '2 )'
      #9#9'{'
      #9#9#9
      'valid = false;'
      #9#9'}'
      #9#9'else'
      #9#9'{'
      #9#9#9
      'var parts = inputval.split'
      '('#39'@'#39');'
      #9#9#9
      'for( var i=0; i < '
      'parts.length; i++ )'
      #9#9#9
      '{'
      #9#9#9
      #9'if( typeof( parts'
      '[i] ) == '#39'undefined'#39' || '
      'parts[i] == '#39#39' )'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'valid = '
      'false;'
      #9#9#9
      #9'}'
      #9#9#9
      '}'
      #9#9#9
      'if( !/[a-zA-Z0-9_\.!#$'
      '%&'#39'*+\/=?\^`{|}'
      '~-]+/.test( parts[0] ) ||'
      #9#9#9
      #9'!/^[a-zA-Z0-'
      '9_!#$%&'#39'*+\/=?\^`{|}'
      '~-]/.test( parts[0] ) ||'
      #9#9#9
      #9'!/[a-zA-Z0-9_!#'
      '$%&'#39'*+\/=?\^`{|}'
      '~-]$/.test( parts[0] ) ||'
      #9#9#9
      #9'!validhostname( '
      'parts[1] ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      ''
      #9'if( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validtimeslot( field, '
      'blank )'
      '{'
      #9'var inputval = '
      'field;'
      #9'var valid = true;'
      ''
      #9'if( '
      'document.getElementById( '
      'field ) )'
      #9'{'
      #9#9
      'inputval = '
      'document.getElementById( '
      'field ).value;'
      #9'}'
      ''
      #9'if( inputval == '#39#39' '
      '&& blank == '#39'false'#39' )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      ''
      #9'var timeslots = '
      'inputval.split( /\n/ );'
      ''
      #9'for( var i=0, len '
      '= timeslots.length; i < len; '
      'i++ )'
      #9'{'
      #9#9'if( '
      'timeslots[i] == undefined '
      '|| timeslots[i] == '#39#39' )'
      #9#9'{'
      #9#9#9
      'continue;'
      #9#9'}'
      #9#9'var '
      'current_timeslot = '
      'timeslots[i];'
      #9#9'if( /^'
      '(Mon|Monday|Tue|'
      'Tuesday|Wed|'
      'Wednesday|Thu|'
      'Thursday|Fri|Friday|Sat|'
      'Saturday|Sun|Sunday):'
      '(\d{1,2}):(\d{1,2})->(\d'
      '{1,2}):(\d{1,2})$/i.test( '
      'current_timeslot ) )'
      #9#9'{'
      #9#9#9
      'var timeslot = '
      'current_timeslot.substr( '
      'current_timeslot.indexOf( '
      #39':'#39' ) + 1 ).split( '#39'->'#39' );'
      #9#9#9
      'var start = timeslot'
      '[0].split( '#39':'#39' );'
      #9#9#9
      'var end = timeslot[1].split( '
      #39':'#39' );'
      ''
      #9#9#9
      'if( start[0] >= 24 || end[0] '
      '> 24 ||'
      #9#9#9
      #9'start[1] > 59 || '
      'end > 59 ||'
      #9#9#9
      #9'( start[0] == 24 '
      '&& end[1] > 0 ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      '}'
      ''
      #9#9#9
      'if( start[0] == end[0] && '
      'start[1] == end[1] )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      '}'
      #9#9'}'
      #9#9'else'
      #9#9'{'
      #9#9#9
      'valid = false;'
      #9#9'}'
      #9'}'
      ''
      #9'if( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function select_all( '
      'element, prefix )'
      '{'
      #9'var i = 0;'
      #9'while ( '
      'document.getElementById'
      '(prefix+i) )'
      #9'{'
      #9#9
      'document.getElementById'
      '(prefix+i).checked = '
      'element.checked;'
      #9#9'i++;'
      #9'}'
      '}'
      ''
      'function xmlGet( url, runme '
      ')'
      '{'
      #9'var xmlHttpReq;'
      ''
      #9'if '
      '(window.XMLHttpRequest)'
      #9'{'
      #9#9'// '
      'Mozilla/Safari'
      #9#9
      'xmlHttpReq = new '
      'XMLHttpRequest();'
      #9'}'
      #9'else if '
      '(window.ActiveXObject)'
      #9'{'
      #9#9'// IE'
      #9#9
      'xmlHttpReq = new '
      'ActiveXObject'
      '("Microsoft.XMLHTTP");'
      #9'}'
      ''
      #9
      'xmlHttpReq.open('#39'GET'#39', '
      'url, true);'
      #9
      'xmlHttpReq.setRequestHe'
      'ader('#39'Content-Type'#39', '
      #39'application/x-www-form-'
      'urlencoded'#39');'
      ''
      #9
      'xmlHttpReq.onreadystatec'
      'hange = function()'
      #9'{'
      #9#9'if ( '
      'xmlHttpReq && '
      'xmlHttpReq.readyState '
      '== 4)'
      #9#9'{'
      #9#9#9
      'runme += "( '
      'xmlHttpReq.responseText '
      ');";'
      #9#9#9
      'eval ( runme );'
      #9#9'}'
      #9'}'
      ''
      #9'try'
      #9'{'
      #9#9
      'xmlHttpReq.send( null );'
      #9'}'
      #9'catch (exception)'
      #9'{'
      #9#9'/* '
      'something went wrong ... ! '
      '*/'
      #9#9'error = '
      '1;'
      #9'}'
      ''
      #9'delete '
      'xmlHttpReq;'
      #9'delete url;'
      '}'
      ''
      'function smoothGet( url, '
      'runme )'
      '{'
      #9'var self = this;'
      ''
      #9'if '
      '(window.XMLHttpRequest)'
      #9'{'
      #9#9'// '
      'Mozilla/Safari'
      #9#9
      'self.xmlHttpReq = new '
      'XMLHttpRequest();'
      #9'}'
      #9'else if '
      '(window.ActiveXObject)'
      #9'{'
      #9#9'// IE'
      #9#9
      'self.xmlHttpReq = new '
      'ActiveXObject'
      '("Microsoft.XMLHTTP");'
      #9'}'
      ''
      #9
      'self.xmlHttpReq.open'
      '('#39'GET'#39', url, true);'
      #9
      'self.xmlHttpReq.setReques'
      'tHeader('#39'Content-Type'#39', '
      #39'application/x-www-form-'
      'urlencoded'#39');'
      ''
      #9
      'self.xmlHttpReq.onreadyst'
      'atechange = function()'
      #9'{'
      #9#9'if ( '
      'self.xmlHttpReq && '
      'self.xmlHttpReq.readyStat'
      'e == 4)'
      #9#9'{'
      #9#9#9
      'runme += "( '
      'self.xmlHttpReq.responseT'
      'ext, url );";'
      #9#9#9
      'eval ( runme );'
      #9#9'}'
      #9'}'
      ''
      #9'try'
      #9'{'
      #9#9
      'self.xmlHttpReq.send( null '
      ');'
      #9'}'
      #9'catch (exception)'
      #9'{'
      #9#9'/* '
      'something went wrong ... ! '
      '*/'
      #9#9
      'haserror = 1;'
      #9'}'
      ''
      #9'delete self;'
      #9'delete url;'
      '}'
      ''
      'var urldetails = new Array'
      '();'
      ''
      'function urlDetailer( id, '
      'initial, source, before, '
      'after )'
      '{'
      #9'if ( !urldetails[ id '
      '] )'
      #9'{'
      #9#9
      'urldetails[ id ] = new '
      'Array( before, after, initial '
      ');'
      #9#9'if ( '
      'source && source != "" )'
      #9#9'{'
      #9#9#9
      'smoothGet( source, '
      '"urlDetailerUpdate" );'
      #9#9'}'
      #9'}'
      ''
      #9'Tip( urldetails[ id '
      '][ 2 ] );'
      '}'
      ''
      'function urlDetailerUpdate( '
      'response, caller )'
      '{'
      #9'var details = '
      'response.split( "~" );'
      ''
      #9'/* the notation '
      'for the details array '
      'changes depending upon '
      'what we'#39're looking at */'
      #9'/* however, the '
      'first few notes are the '
      'same */'
      #9'/* type, id */'
      ''
      #9'var id = details[ '
      '0 ] + details[ 1 ];'
      #9'var before = '
      'urldetails[ id ][ 0 ];'
      #9'var after  = '
      'urldetails[ id ][ 1 ];'
      ''
      #9'if ( details[ 0 ] '
      '== "youtube" )'
      #9'{'
      #9#9'/* '
      'parse youtube details ... */'
      #9#9
      'content = before;'
      #9#9
      'content += "<tr><td '
      'colspan='#39'2'#39' style='#39'text-'
      'align: center; font-weight: '
      'bold;'#39'>YouTube Video - " + '
      'details[ 2 ] + '
      '"</td></tr>";'
      #9#9
      'content += "<tr><td '
      'colspan='#39'2'#39' style='#39'text-'
      'align: center;'#39'><img src='#39'" '
      '+ details[ 3 ] + "'#39' '
      'style='#39'height: " + details[ 4 '
      '] + "px; width: " + details[ '
      '5 ] + "px;'#39' alt='#39'" + details[ '
      '3 ] + "'#39'></td></tr>";'
      #9#9
      'content += "<tr><td '
      'colspan='#39'2'#39' style='#39'text-'
      'align: center; font-size: '
      '7pt; color: #404040;'#39'>" + '
      'details[ 6 ] + '
      '"</td></tr>";'
      #9#9
      'content += after;'
      #9'}'
      #9'else if ( details[ 0 '
      '] == "whois" )'
      #9'{'
      #9#9
      'content = before;'
      #9#9
      'content += details[ 2 ];'
      #9#9
      'content += after;'
      #9'}'
      ''
      #9'urldetails[ id ][ 2 '
      '] = content;'
      #9'Tip( urldetails[ id '
      '][ 2 ] );'
      '}'
      ''
      'function toggleTree( id )'
      '{'
      #9'var element    = '
      'document.getElementById( '
      'id );'
      #9'var indicator  = '
      'document.getElementById( '
      'id + '#39'_node'#39' );'
      #9'var historical = '
      'document.getElementById( '
      'id + '#39'_expanded'#39' );'
      ''
      #9'if ( element )'
      #9'{'
      #9#9'if( '
      'element.style.display == '
      #39'none'#39' )'
      #9#9'{'
      #9#9#9
      'element.style.display = '
      #39'inline'#39';'
      #9#9#9
      'indicator.src = '
      #39'/themes/default/treeclose.'
      'png'#39';'
      #9#9#9
      'historical.checked = true;'
      #9#9'}'
      #9#9'else'
      #9#9'{'
      #9#9#9
      'element.style.display = '
      #39'none'#39';'
      #9#9#9
      'indicator.src = '
      #39'/themes/default/treeopen.'
      'png'#39';'
      #9#9#9
      'historical.checked = false;'
      #9#9'}'
      #9'}'
      '}'
      ''
      'var __toggleTreeSelectLast '
      '= -1;'
      ''
      'function '
      'toggleTreeSelectDown( ev, '
      'tree, num, id )'
      '{'
      #9'var key = '
      'ev.shiftKey;'
      ''
      #9'var element   = '
      'document.getElementById( '
      'tree + '#39'_tree'#39' );'
      #9'var indicator = '
      'document.getElementById( '
      'tree + '#39'_nodule_'#39' + num );'
      ''
      #9
      'toggleTreeSelectMark( '
      'element, num, indicator );'
      ''
      #9'if ( key != true )'
      #9'{'
      #9#9'// don'#39't '
      'do this if they'#39've pressed '
      'select'
      #9#9
      '__toggleTreeSelectLast = '
      'num;'
      #9'}'
      '}'
      ''
      '/* URL higlighting */'
      ''
      'function '
      'updateURLHighlight( vid )'
      '{'
      #9'var typedurl = '
      'document.getElementById( '
      'vid ).value;'
      ''
      #9'var match = /^'
      '(([^:]*):\/\/)?([^\/]*)'
      '(\/?.*)$/.exec( typedurl );'
      ''
      #9
      'document.getElementById( '
      'vid + '#39'_highlight'#39' '
      ').innerHTML = "";'
      #9'if ( match[ 1 ] )'
      #9'{'
      #9#9
      'document.getElementById( '
      'vid + '#39'_highlight'#39' '
      ').innerHTML +=  "<span '
      'style='#39'padding: 0px; color: '
      '#BCD2EE; font-size: 10pt; '
      'font-family: \"MS Verdana'
      '\", sans-serif; font-weight : '
      'normal; background-color: '
      '#BCD2EE;'#39'>" + match[ 1 ] '
      '+ "</span>";'
      #9'}'
      #9'if ( match[ 3 ] )'
      #9'{'
      #9#9
      'document.getElementById( '
      'vid + '#39'_highlight'#39' '
      ').innerHTML += "<span '
      'style='#39'padding: 0px; color: '
      '#DAF4F0; font-size: 10pt; '
      'font-family: \"MS Verdana'
      '\", sans-serif; font-weight : '
      'normal; background-color: '
      '#DAF4F0;'#39'>" + match[ 3 ] '
      '+ "</span>";'
      #9'}'
      #9'if ( match[ 4 ] )'
      #9'{'
      #9#9
      'document.getElementById( '
      'vid + '#39'_highlight'#39' '
      ').innerHTML += "<span '
      'style='#39'padding: 0px; color: '
      '#FFEFD5; font-size: 10pt; '
      'font-family: \"MS Verdana'
      '\", sans-serif; font-weight : '
      'normal; background-color: '
      '#FFEFD5;'#39'>" + match[ 4 ] '
      '+ "</span>";'
      #9'}'
      #9
      'document.getElementById( '
      'vid + '#39'_highlight'#39' '
      ').innerHTML += "&nbsp;";'
      '}'
      ''
      ''
      'function '
      'toggleTreeSelectUp( ev, '
      'tree, num, id )'
      '{'
      #9'var element   = '
      'document.getElementById( '
      'tree + '#39'_tree'#39' );'
      #9'var indicator = '
      'document.getElementById( '
      'tree + '#39'_nodule_'#39' + num );'
      ''
      #9'var key = '
      'ev.shiftKey;'
      #9'var endpoint = '
      'num;'
      ''
      #9'if ( key == true )'
      #9'{'
      #9#9'num --;'
      #9'}'
      ''
      #9'for ( var i = ( '
      '__toggleTreeSelectLast + '
      '1 ) ; i <= num ; i++ )'
      #9'{'
      #9#9
      'indicator = '
      'document.getElementById( '
      'tree + '#39'_nodule_'#39' + i );'
      #9#9
      'toggleTreeSelectMark( '
      'element, i, indicator );'
      #9'}'
      ''
      #9
      '__toggleTreeSelectLast = '
      'endpoint;'
      '}'
      ''
      'function '
      'toggleTreeSelectMark( '
      'element, i, indicator, stats '
      ')'
      '{'
      #9'if ( element )'
      #9'{'
      #9#9'if( '
      'element.options[ i '
      '].selected == true )'
      #9#9'{'
      #9#9#9
      'indicator.style.background'
      'Color = '#39'#ffffff'#39';'
      #9#9#9
      'element.options[ i '
      '].selected = false;'
      #9#9'}'
      #9#9'else'
      #9#9'{'
      #9#9#9
      'indicator.style.background'
      'Color = '#39'#c0c0ff'#39';'
      #9#9#9
      'element.options[ i '
      '].selected = true;'
      #9#9'}'
      #9'}'
      '}'
      ''
      '/* tabview control */'
      ''
      'function selectTab( uname, '
      'selopt )'
      '{'
      #9'/* get the '
      'current selected tab */'
      #9'var opts = '
      'document.getElementsByN'
      'ame( uname );'
      ''
      #9'for ( var i = 0; i '
      '< opts.length ; i++ )'
      #9'{'
      #9#9'if ( '
      'opts[ i ].value == selopt )'
      #9#9'{'
      #9#9#9
      'opts[ i ].checked = true;'
      #9#9#9
      'document.getElementById( '
      'uname + '#39'_'#39' + '#9'opts[ i '
      '].value ).style.display = '
      #39'inline'#39';'
