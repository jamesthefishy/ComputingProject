      'tor"];}'
      'f.prototype.overloadSetter'
      '=function(v){var i=this;'
      'return function(x,w){if'
      '(x==null){return this;}if'
      '(v||typeof x!="string")'
      '{for(var y in x){i.call'
      '(this,y,x[y]);}if(r){for(var '
      'z=r.length;z--;){y=r[z];if'
      '(x.hasOwnProperty(y))'
      '{i.call(this,y,x[y]);'
      '}}}}else{i.call(this,x,w);}'
      'return '
      'this;};};f.prototype.overlo'
      'adGetter=function(v){var '
      'i=this;return function(x)'
      '{var y,w;if(typeof x!'
      '="string"){y=x;}else{if'
      '(arguments.length>1)'
      '{y=arguments;'
      '}else{if(v){y=[x];}}}if(y)'
      '{w={};for(var '
