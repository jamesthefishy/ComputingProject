      'ON.decode'
      '(c,this.options.secure);}'
      'catch(a){this.fireEvent'
      '("error",[c,a]);return;}if'
      '(b==null){this.onFailure();'
      '}else{this.onSuccess'
      '(b,c);}}});var Cookie=new '
