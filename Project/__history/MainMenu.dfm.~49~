      #9#9'}'
      #9#9'else'
      #9#9'{'
      #9#9#9
      'document.getElementById( '
      'uname + '#39'_'#39' + '#9'opts[ i '
      '].value ).style.display = '
      #39'none'#39';'
      #9#9#9
      'document.getElementById( '
      'uname + '#39'_'#39' + '#9'opts[ i '
      '].value + '#39'_title'#39' '
      ').style.backgroundColor = '
      #39'#e7e7e7'#39';'
      #9#9#9
