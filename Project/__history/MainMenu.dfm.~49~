      'field );'
      #9'} else {'
      #9#9'_error( '
      'field );'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function '
      'validipandportvalue( '
      'ipandport )'
      '{'
      #9'var valid = false;'
      ''
      #9'/* split it into a '
      'number and a port */'
      #9'var detail_finder '
      '= new RegExp( /^(.*?):'
      '(.*?)$/ );'
      #9'var matches = '
      'detail_finder.exec( '
      'ipandport );'
      #9'if( matches )'
      #9'{'
      #9#9'if( '
      'validipvalue( matches[1] ) '
      '&&'
      #9#9#9
      'validport( matches[2], '
      #39'false'#39' ) )'
      #9#9'{'
      #9#9#9
      'valid = true;'
      #9#9'}'
      #9'}'
      ''
      #9'return valid;'
      '}'
      ''
      'function '
      'validipandportorhostname'
      '(field, blank)'
      '{'
      #9'var '
      'ipandportorhostname = '
      'document.getElementById'
      '(field).value;'
      #9'var valid = false;'
      ''
      #9'if ( '
      'ipandportorhostname == '
      '"" && blank == '#39'true'#39' )'
      #9'{'
      #9#9'valid = '
