      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function '
      'validcategorynamevalue'
      '(inputval, blank)'
      '{'
      #9'if( blank == '
      'undefined || blank != '#39'true'#39' '
      ') { blank = '#39'false'#39'; }'
      ''
      #9'var valid = false;'
      ''
      #9'if( inputval == '
      'undefined || inputval == "" '
      ')'
      #9'{'
      #9#9'// drop '
      'in here if undefined so we '
      'dont fall through to else'
      #9#9'if( '
      'blank == '#39'true'#39' )'
      #9#9'{'
      #9#9#9
      'valid = true;'
      #9#9'}'
      #9'}'
      #9'else // defined '
      'and not an empty string'
      #9'{'
      #9#9'if( '
      'inputval.length < 255 &&'
      #9#9#9
      '!/[\\'#39'"\(\)\{\}\<\>]/.test'
      '(inputval) )'
      #9#9'{'
      #9#9#9
      'valid = true;'
      #9#9'}'
      #9'}'
      ''
      #9'return valid;'
      '}'
      ''
      'function validportrange( '
      'field, blank )'
      '{'
      #9'var port = field;'
      #9'if ( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9'port = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'var valid = false;'
      #9
      #9'if ( validport'
      '(field, blank) )'
      #9'{'
      #9#9'valid = '
      'true;'
      #9'}'
      #9'else'
      #9'{'
      #9#9'var '
      'detail_finder = new '
      'RegExp( /^(\d*):(\d*)$/ );'
      #9#9'var '
      'matches = '
      'detail_finder.exec( port );'
      #9#9'if ( '
      'matches )'
      #9#9'{'
      #9#9#9
      'var p1 = parseInt( '
      'matches[1] );'
      #9#9#9
      'var p2 = parseInt( '
      'matches[2] );'
      #9#9#9
      'if ( validport( p1 ) && '
      'validport( p2 ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'if ( p1 < p2 )'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'valid = '
      'true;'
      #9#9#9
      #9'}'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      ''
      #9'if ( valid ){'
      #9#9'_ok( '
      'field );'
      #9'} else {'
      #9#9'_error( '
      'field );'
      #9'}'
      #9'return valid;'
      '}'
      ''
      ''
      'function validmac( field, '
      'blank )'
      '{'
      #9'var inputval = '
      'field;'
      #9'if ( '
      'document.getElementById'
      '(field) ){'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'var valid = false;'
      ''
      #9'var errored = '
      '!/^[0-9a-fA-F]{2}[\:\-][0-'
      '9a-fA-F]{2}[\:\-][0-9a-fA-'
      'F]{2}[\:\-][0-9a-fA-F]{2}'
      '[\:\-][0-9a-fA-F]{2}[\:\-]'
      '[0-9a-fA-F]{2}$/.test'
      '(inputval);'
      ''
      #9'if(errored)'
      #9'{'
      #9#9'valid = '
      'true;'
      #9'}'
      #9'if( inputval == "" '
      '&& blank == true)'
      #9'{'
      #9#9'valid = '
      'true;'
      #9'}'
      ''
      #9'if ( valid ){'
      #9#9'_error'
      '(field);'
      #9'} else {'
      #9#9'_ok'
      '(field);'
      #9'}'
      '}'
      ''
      'function validname( field, '
      'blank, spaces, safe )'
      '{'
      #9'var name = '
      'document.getElementById'
      '(field).value;'
      ''
      #9'var valid = true;'
      ''
      #9'if( /\s/.test( '
      'name ) && spaces == '
      #39'false'#39' )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'else if( name == '
      '"" && blank == '#39'false'#39' )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'else if( name != '
      '"" )'
      #9'{'
      #9#9'if( '
      'name.length > 255 )'
      #9#9'{'
      #9#9#9
      'valid = false;'
      #9#9'}'
      #9#9'else if( '
      '!/^[a-z0-9\-_\.\s]+'
      '$/i.test(name) && safe == '
      #39'true'#39' )'
      #9#9'{'
      #9#9#9
      'valid = false;'
      #9#9'}'
      #9#9'else if( '
      '!/^[\x20-\x7E]+$/.test'
      '(name) || /[<>]/.test'
      '(name) )'
      #9#9'{'
      #9#9#9
      'valid = false;'
      #9#9'}'
      #9'}'
      ''
      #9'if ( valid ){'
      #9#9'_ok( '
      'field );'
      #9'} else {'
      #9#9'_error( '
      'field );'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validcomment( '
      'field,blank,max_length )'
      '{'
      #9'var inputval = '
      'field;'
      #9'if ( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      #9'var valid = true;'
      ''
      #9'if( ( typeof( '
      'inputval ) == '#39'undefined'#39' || '
      'inputval == '#39#39' ) && blank '
      '== '#39'false'#39' )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'else if( /'
      '[<>]/.test(inputval) )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      #9'else if( '
      'inputval.length > '
      'max_length )'
      #9'{'
      #9#9'valid = '
      'false;'
      #9'}'
      ''
      #9'if ( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      '}'
      ''
      ''
      'function validhostname( '
      'field, blank, wildcard )'
      '{'
      #9'var valid = true;'
      #9'var inputval = '
      'field;'
      ''
      #9'if ( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      ''
      #9'if( ( inputval == '#39#39' '
      '|| typeof( inputval ) == '
      #39'undefined'#39' ) && blank == '
      #39'true'#39' )'
      #9'{'
      #9#9'valid = '
      'true;'
      #9'}'
      #9'else if( typeof( '
      'inputval ) != '#39'undefined'#39' )'
      #9'{'
      #9#9'var '
      'hostnames = '
      'inputval.replace(/\r/g,"");'
      #9#9
      'hostnames = '
      'hostnames.split("\n");'
      #9#9'var '
      'hostnameslength = '
      'hostnames.length;'
      ''
      #9#9'for( '
      'var i = 0 ; i < '
      'hostnameslength ; i++ )'
      #9#9'{'
      #9#9#9
      '// see rfc1035 - (Domain '
      'names)'
      #9#9#9
      '// also see windows xp '
      'network id '
      'recommendations'
      #9#9#9
      '// also not allowing '
      'hostnames with just a '
      'number'
      #9#9#9
      'if( /^[0-9.*?-]+$/.test( '
      'hostnames[i] ) || /^[-'
      '\.]/.test( hostnames[i] ) ||'
      #9#9#9
      #9#9'/[-'
      '\.]$/.test( hostnames[i] ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9#9
      'if( ( hostnames[i].length < '
      '1 ) || ( hostnames[i].length '
      '> 255 ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = false;'
      #9#9#9
      #9'break;'
      #9#9#9
      '}'
      #9#9#9
      'else'
      #9#9#9
      '{'
      #9#9#9
      #9'var parts = '
      'hostnames[i].split(".");'
      #9#9#9
      #9'var partlength = '
      'parts.length;'
      #9#9#9
      #9'for(var part = 0 ; '
      'part < partlength ; part+'
      '+)'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9'if( ( '
      'parts[part].length < 1 ) || ( '
      'parts[part].length > 63 ) )'
      #9#9#9
      #9#9'{'
      #9#9#9
      #9#9#9
      'valid = false;'
      #9#9#9
      #9#9#9
      'break;'
      #9#9#9
      #9#9'}'
      #9#9#9
      #9#9'if( ( !'
      '(/^[a-zA-Z0-9](?:[a-zA-Z0'
      '-9-]*[a-zA-Z0-9])?$/.test'
      '(parts[part])) ||'
      #9#9#9
      #9#9#9
      #9'/\s/.test(parts'
      '[part]) ) && wildcard != '
      #39'true'#39' )'
      #9#9#9
      #9#9'{'
      #9#9#9
      #9#9#9
      'valid = false;'
      #9#9#9
      #9#9#9
      'break;'
      #9#9#9
      #9#9'}'
      #9#9#9
      #9#9'else if( '
      '( !(/^[a-zA-Z0-9?*](?:[a-'
      'zA-Z0-9?*-]*[a-zA-Z0-9?'
      '*])?$/.test(parts[part])) ||'
      #9#9#9
      #9#9#9
      #9'/\s/.test(parts'
      '[part]) || /[*]{2,}/.test'
      '(parts[part])) && wildcard '
      '== '#39'true'#39' )'
      #9#9#9
      #9#9'{'
      #9#9#9
      #9#9#9
      'valid = false;'
      #9#9#9
      #9#9#9
      'break;'
      #9#9#9
      #9#9'}'
      #9#9#9
      #9'}'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      ''
      #9'if ( !valid ){'
      #9#9#9
      '_error(field);'
      #9'} else {'
      #9#9#9
      '_ok(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function validpacaddress( '
      'field, blank )'
      '{'
      #9'var inputval = '
      'field;'
      #9'if ( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
