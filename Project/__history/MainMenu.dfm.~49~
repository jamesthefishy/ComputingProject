      ''
      'function '
      'validiporiprangeoripandmas'
      'korhostname(field, blank, '
      'wildcards)'
      '{'
      #9'var inputval = '
      'field;'
      #9'if ( '
      'document.getElementById'
      '(field) )'
      #9'{'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      #9'var valid = false;'
      ''
      #9'if ( inputval == "" '
      '&& blank == '#39'true'#39' )'
      #9'{'
      #9#9'valid = '
      'true;'
      #9'}'
      #9'else'
      #9'{'
      #9#9
      'inputval = inputval.split(/'
      '\n|\r/);'
      #9#9'var '
      'ip_hostnames = new '
      'Array();'
      #9#9'for( '
      'var i=0; i < '
      'inputval.length; i++ )'
      #9#9'{'
      #9#9#9
      'if( ( inputval[i].length == 0 '
      ') || ( inputval[i] == '#39'\n'#39' ) ) { '
      'continue };'
      #9#9#9
      'ip_hostnames.push( '
      'inputval[i] );'
      #9#9'}'
      #9#9'var '
      'ip_hostnameslength = '
      'ip_hostnames.length;'
      ''
      #9#9'var '
      'valid_count = 0;'
      #9#9'for( '
      'var counter=0; counter < '
      'ip_hostnameslength; '
      'counter++ )'
      #9#9'{'
      #9#9#9
      'if( validipvalue( '
      'ip_hostnames[counter] ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid_count++;'
      #9#9#9
      '}'
      #9#9#9
      'else if( validiprange( '
      'ip_hostnames[counter] ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid_count++;'
      #9#9#9
      '}'
      #9#9#9
      'else if( validipandmask( '
      'ip_hostnames[counter] ) )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid_count++;'
      #9#9#9
      '}'
      #9#9#9
      'else if( validhostname( '
      'ip_hostnames'
      '[counter],blank,wildcards ) '
      ')'
      #9#9#9
      '{'
      #9#9#9
      #9'valid_count++;'
      #9#9#9
      '}'
      #9#9'}'
      #9#9'if( '
      'valid_count == '
      'ip_hostnameslength )'
      #9#9'{'
      #9#9#9
      'valid = true;'
      #9#9'}'
      #9'}'
      ''
      #9'if ( valid ){'
      #9#9'_ok( '
      'field );'
      #9'} else {'
      #9#9'_error( '
      'field );'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function '
      'validiporipandmaskoriprang'
      'e(field, blank)'
      '{'
      #9'var inputval = '
      'field;'
      #9'if ( '
      'document.getElementById'
      '(field) ){'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      #9'var valid = '
      'validiporipandmaskoriprang'
      'evalue( inputval, blank );'
      ''
      #9'if ( valid ){'
      #9#9'_ok'
      '(field);'
      #9'} else {'
      #9#9'_error'
      '(field);'
      #9'}'
      #9'return valid;'
      '}'
      ''
      'function '
      'validiporipandmaskoriprang'
      'evalue(inputval, blank)'
      '{'
      #9'if( blank == '
      'undefined || blank != '#39'true'#39' '
      ') { blank = '#39'false'#39'; }'
      ''
      #9'var valid = false;'
      ''
      #9'if( inputval == '
      'undefined || inputval == "" '
      ')'
      #9'{'
      #9#9'// drop '
      'in here if undefined so we '
      'dont fall through to else'
      #9#9'if( '
      'blank == '#39'true'#39' )'
      #9#9'{'
      #9#9#9
      'valid = true;'
      #9#9'}'
      #9'}'
      #9'else // defined '
      'and not an empty string'
      #9'{'
      #9#9
      'inputval = inputval.split(/'
      '\n|\r/);'
      #9#9'var '
      'addresses = new Array();'
      #9#9'for( '
      'var i=0; i < '
      'inputval.length; i++ )'
      #9#9'{'
      #9#9#9
      'if( ( inputval[i].length == 0 '
      ') || ( inputval[i] == '#39'\n'#39' ) ) { '
      'continue; }'
      #9#9#9
      'addresses.push( inputval[i] '
      ');'
      #9#9'}'
      #9#9'var '
      'addresseslength = '
      'addresses.length;'
      ''
      #9#9'if( '
      'addresseslength && '
      'addresseslength > 0 )'
      #9#9'{'
      #9#9#9
      'var counter = 0;'
      #9#9#9
      'var failed = false;'
      ''
      #9#9#9
      '// check each individual '
      'entry'
      #9#9#9
      '// if any fails validation set '
      'flag and immediately stop '
      'checking further'
      #9#9#9
      'do'
      #9#9#9
      '{'
      #9#9#9
      #9'if( validip( '
      'addresses[ counter ] ) ||'
      #9#9#9
      #9#9
      'validipandmask( addresses[ '
      'counter ] ) ||'
      #9#9#9
      #9#9
      'validiprange( addresses[ '
      'counter ] ) )'
      #9#9#9
      #9'{'
      #9#9#9
      #9#9
      'counter++;'
      #9#9#9
      #9#9
      'continue;'
      #9#9#9
      #9'}'
      #9#9#9
      #9'failed = true; // if '
      'we get here we need to '
      'break out'
      #9#9#9
      '}while( ( counter < '
      'addresseslength ) && ( '
      'failed != true ) )'
      ''
      #9#9#9
      'if( !failed )'
      #9#9#9
      '{'
      #9#9#9
      #9'valid = true;'
      #9#9#9
      '}'
      #9#9'}'
      #9'}'
      ''
      #9'return valid;'
      '}'
      ''
      'function '
      'validcategoryname(field, '
      'blank)'
      '{'
      #9'var inputval = '
      'field;'
      #9'if ( '
      'document.getElementById'
      '(field) ){'
      #9#9
      'inputval = '
      'document.getElementById'
      '(field).value;'
      #9'}'
      #9'var valid = '
      'validcategorynamevalue( '
      'inputval, blank );'
