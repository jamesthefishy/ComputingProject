      '// Copyright 2004-2013 '
      'Smoothwall Ltd'
      ''
      'function '
      'MM_preloadImages() {'
      '  var d=document; if'
      '(d.images){ if(!d.MM_p) '
      'd.MM_p=new Array();'
      '    var '
      'i,j=d.MM_p.length,a=MM_'
      'preloadImages.arguments; '
      'for(i=0; i<a.length; i++)'
      '    if (a[i].indexOf("#")!'
      '=0){ d.MM_p[j]=new '
      'Image; d.MM_p[j+'
      '+].src=a[i];}}'
      '}'
      ''
      'function '
      'MM_swapImgRestore() {'
      '  var '
      'i,x,a=document.MM_sr; '
      'for(i=0;a&&i<a.length&&'
      '(x=a[i])&&x.oSrc;i++) '
      'x.src=x.oSrc;'
      '}'
      ''
      'function MM_findObj(n, d) '
      '{'
      '  var p,i,x;  if(!d) '
      'd=document; if'
      '((p=n.indexOf'
      '("?"))>0&&parent.frames.l'
      'ength) {'
      '    d=parent.frames'
      '[n.substring(p'
      '+1)].document; '
      'n=n.substring(0,p);}'
      '  if(!(x=d[n])&&d.all) '
      'x=d.all[n]; for (i=0;!'
      'x&&i<d.forms.length;i++) '
      'x=d.forms[i][n];'
      '  for(i=0;!'
      'x&&d.layers&&i<d.layers.l'
      'ength;i++) x=MM_findObj'
      '(n,d.layers[i].document); '
      'return x;'
      '}'
      ''
      'function MM_swapImage() '
      '{'
      '  var '
      'i,j=0,x,a=MM_swapImage.'
      'arguments; '
      'document.MM_sr=new '
      'Array; for(i=0;i<'
      '(a.length-2);i+=3)'
      '   if ((x=MM_findObj(a'
      '[i]))!=null)'
      '{document.MM_sr[j++]=x; '
      'if(!x.oSrc) x.oSrc=x.src; '
      'x.src=a[i+2];}'
      '}'
      ''
      'function displayHelp(url, '
      'page) {'
      #9'if ( !page ){'
      #9#9'page = '
      '"/cgi-bin/help.cgi/";'
      #9'}'
      #9'window.open'
      '(page + "?" '
      '+url,"disposableHelpWindo'
      'w","resizable=yes,status='
      'no,scrollbars=yes,width=4'
      '00,height=400");'
      '}'
      ''
      '// menu functions ... these '
      'are a bit fruity ..'
      ''
      'function nav_menu( e, loce '
      ')'
      '{'
      #9'var button;'
      ''
      #9'if ( !e ){'
      #9#9'e = '
      'window.event;'
      #9'}'
      ''
      #9'if ( e.which == '
      'null) {'
      '       '#9#9'/* IE '
      'case */'
      #9#9'button '
      '= ( e.button < 2) ? "LEFT" '
      ': (( e.button == 4) ? '
      '"MIDDLE" : "RIGHT");'
      #9'} else {'
      '       '#9#9'/* All '
      'others */'
      '       '#9#9'button '
      '= ( e.which < 2) ? "LEFT" :  '
      '(( e.which == 2) ? '
      '"MIDDLE" : "RIGHT");'
      #9'}'
      ''
      #9'if ( button == '
      '"LEFT" ){'
      #9#9
      'window.location.href = '
      'loce;'
      #9'} else if ( button '
      '== "MIDDLE" ){'
      #9#9'var '
      'nme = "sw" + Math.floor'
      '(Math.random()*200+1)'
      #9#9'var '
      'window2 = window.open( '
      'loce, nme );'
      #9#9
      'window2.blur();'
      #9#9
      'window.focus();'
      #9'}'
      '}'
      ''
      '// Function to select (mark) '
      'a row by clicking on it'
      ''
      'function toggle_row( id )'
      '{'
      #9'if ( '
      'document.getElementById'
      '(id).checked == true ){'
      #9#9
      'document.getElementById'
      '(id).checked = false;'
      #9'} else {'
      #9#9
      'document.getElementById'
      '(id).checked = true;'
      #9'}'
      '}'
      ''
      '// function to collapse '
      'boxes.'
      ''
      'function toggle_collapse( id '
      ')'
      '{'
      #9'if ( !'
      'document.getElementById'
      '(id) ){'
      #9#9'return;'
      #9'}'
      ''
      '    var el;'
      #9'if ( jQuery ){'
      #9#9'el = '
      'jQuery('#39'#'#39' + id).prev('#39'h1'#39');'
      #9'}'
      ''
      #9'if ( '
      'document.getElementById'
      '(id).style.display == '#39'none'#39' '
      '){'
      #9#9'if ( el ){'
      #9#9#9
      'el.addClass'
      '('#39'open_arrow'#39').removeClas'
      's('#39'closed_arrow'#39');'
      #9#9'}'
      #9#9
      'document.getElementById'
      '(id).style.display = '#39#39';'
      #9'} else {'
      #9#9'if ( el ){'
      #9#9#9
      'el.addClass'
      '('#39'closed_arrow'#39').removeCla'
      'ss('#39'open_arrow'#39');'
      #9#9'}'
      #9#9
      'document.getElementById'
      '(id).style.display = '#39'none'#39';'
      #9'}'
      '}'
      ''
      'function show_element( id '
      ')'
      '{'
      #9
      'document.getElementById'
      '(id).style.display = '#39#39';'
      #9'if ( '
      'document.getElementById'
      '(id+'#39'.img'#39')){'
      #9#9
      'document.getElementById'
      '(id+'#39'.img'#39').src = '
      #39'/themes/default/close.jpg'#39';'
      #9'}'
      '}'
      ''
      'function hide_element( id )'
      '{'
      #9
      'document.getElementById'
      '(id).style.display = '#39'none'#39';'
      #9'if ( '
      'document.getElementById'
      '(id+'#39'.img'#39')){'
      #9#9
      'document.getElementById'
      '(id+'#39'.img'#39').src = '
      #39'/themes/default/open.jpg'#39';'
      #9'}'
      '}'
      ''
      'function toggle_hide( id, '
      'ontag, offtag )'
      '{'
      #9'var tag;'
      #9'if ( '
      'document.getElementById'
      '(id).style.display == '#39'none'#39' '
      '){'
      #9#9
      'document.getElementById'
      '(id).style.display = '#39#39';'
      #9#9'tag = '
      'ontag;'
      #9'} else {'
      #9#9
      'document.getElementById'
      '(id).style.display = '#39'none'#39';'
      #9#9'tag = '
      'offtag;'
      #9'}'
      ''
      #9'if ( this.value ){'
      #9#9
      'this.value = tag;'
      #9'} else if ( '
      'this.innerHTML ){'
      #9#9
      'this.innerHTML = tag;'
      #9'}'
      '}'
      ''
      ''
      'function toggleAdvanced( '
      'btn, cap1, cap2 )'
      '{'
      '        var advanced = '
      'document.getElementById( '
      'btn + '#39'_con'#39' );'
      ''
      '        if ( '
      'advanced.style.display == '
      '"none" ){'
      '                '
      'advanced.style.display = '
